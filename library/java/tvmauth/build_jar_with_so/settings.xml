<settings>
    <servers>
        <server>
            <id>artifactory</id>
            <username>yandexpassportdeploy</username>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>me1</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <groupId>ru.yandex.passport</groupId>
                <artifactId>tvmauth-java</artifactId>
                <repositoryId>artifactory</repositoryId>
                <url>http://artifactory.yandex.net/artifactory/yandex_passport</url>
                <file>tvmauth-java-{version}.jar</file>
                <pomFile>tvmauth-java-{version}.pom</pomFile>
                <sources>tvmauth-java-{version}-sources.jar</sources>
            </properties>
        </profile>
    </profiles>
</settings>
