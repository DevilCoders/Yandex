{
    "meta": {
        "name": "nirvana-context",
        "maintainer": "nirvana <nirvana@yandex-team.ru>",
        "description": "Nirvana library with context modules",
        "version": "1.0.0"
    },
    "build": {
        "targets": [
        ]
    },
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "library/python/nirvana",
                "files": ["glob:*.py"]
            },
            "destination": {
                "path": "/nirvana/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "library/python/nirvana/package/setup.py"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "library/python/nirvana/package/setup.cfg"
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
