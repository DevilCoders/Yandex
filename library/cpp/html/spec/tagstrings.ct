%input "grep -oE 'HT_[A-Z][A-Z0-9]*,' tags.h |"
%line-skip 1
%field-separator '_|,'
%pragma-once

#include "tags.h"

struct TTagStrings
{
    HT_TAG Tag;
    unsigned OpenLen;
    const char* Open;
    unsigned CloseLen;
    const char* Close;
};

TTagStrings TagStrings[]={
    { HT_any, 5, "<any>", 6, "</any>" },
  %foreach {
    { HT_$2, %eval { length("$2") + 2 }, "<%eval { lc '$2' }>", %eval { length("$2") + 3 }, "</%eval { lc '$2' }>" },
  %}
};

