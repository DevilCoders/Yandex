{
  "meta": {
    "name": "mstand-binary",
    "maintainer": "ivankun",
    "description": "Metrics Stand (mstand) package",
    "version": "{revision}",
    "homepage": "https://wiki.yandex-team.ru/mstand"
  },
  "build": {
    "targets": [
        "tools/mstand/bin/mstand",
        "tools/mstand/squeeze_lib/bin"
    ],
    "flags": [
      {
        "name": "STRIP",
        "value": "yes"
      }
    ]
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "tools/mstand/bin/mstand/mstand"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "tools/mstand/squeeze_lib/bin/bin"
      },
      "destination": {
        "path": "/squeeze_bin"
      }
    },
    {
      "source": {
        "type": "INLINE",
        "content": "{revision}"
      },
        "destination": {
        "path": "/revision.txt"
      }
    }
  ]
}
