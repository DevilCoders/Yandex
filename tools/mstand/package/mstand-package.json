{
  "meta": {
    "name": "mstand",
    "maintainer": "vdmit",
    "description": "Metrics Stand (mstand) package",
    "version": "{revision}",
    "homepage": "https://wiki.yandex-team.ru/mstand"
  },
  "build": {
    "targets": []
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "tools/mstand",
        "files": [
          "[!.]*.py",
          "[!.]*.sh",
          "templates/*.tpl",
          "templates/*.js",
          "sample_metrics/online/online-lamps-batch.json"
        ]
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "quality/yaqlib",
        "files": [
          "*.sh",
		  "yaq*/*.py",
		  "pytlib/*.py",
		  "*.png"
        ]
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "quality/logs/mousetrack_lib/python",
        "files": [
         "mousetrack_decoder.py"
        ]
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "INLINE",
        "content": "{revision}"
      },
        "destination": {
        "path": "/revision.txt"
      }
    }
  ]
}
