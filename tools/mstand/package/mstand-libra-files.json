{
    "meta": {
        "name": "ext-mstand-libra-files",
        "maintainer": "vdmit",
        "description": "mstand package extension with libra-related files",
        "version": "{revision}",
        "homepage": "https://wiki.yandex-team.ru/mstand"
    },
    "build": {
        "targets": [
            "tools/mstand/bin/yabs_experiment_helper"
        ],
        "flags": [
        {
            "name": "STRIP",
            "value": "yes"
        }
    ]
    },
    "data": [
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": "1106688711",
                "path": "blockstat.dict"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": "1106689888",
                "path": "libra.so"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": "832422087",
                "path": "browser.xml"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": "831515361",
                "path": "geodata4.bin"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "tools/mstand/bin/yabs_experiment_helper",
                "files": [
                    "experiment_helper.so"
                ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
