{
  "meta": {
    "name": "mstand",
    "maintainer": "vdmit",
    "description": "Metrics Stand (mstand) package",
    "version": "{revision}",
    "homepage": "https://wiki.yandex-team.ru/mstand"
  },
  "build": {
    "targets": []
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "tools/mstand",
        "files": [
          "[!.]*.py",
          "[!.]*.sh",
          "templates/*.tpl",
          "templates/*.js",
          "[!.]*.ini",
          "[!.]*/tests/data/*",
          "[!.]*.json",
          "[!.]*.jsonl",
          "[!.]*.tsv",
          "*ya.make"
        ]
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "quality/yaqlib",
        "files": [
          "*.sh",
          "yaq*/*.py",
		  "pytlib/*.py"
        ]
      },
      "destination": {
        "path": "/"
      }
    }
  ]
}
