{
    "meta" : {
        "name" : "nginx-noperl",
        "maintainer" : "Anton Kortunov <toshik@yandex-team.ru>",
        "description" : "small, but very powerful and efficient web server",
        "version" : "{changelog_version}",
        "provides" : ["nginx", "nginx-full", "nginx-light", "nginx-extras", "nginx-common", "nginx-doc"],
        "replaces" : ["nginx", "nginx-full", "nginx-light", "nginx-extras", "nginx-common", "nginx-doc"],
        "conflicts" : ["nginx", "nginx-full", "nginx-light", "nginx-extras", "nginx-common", "nginx-doc"]
    },
    "build" : {
        "targets" :  [
            "nginx/bin-noperl"
        ]
    },
    "data" : [
        {
            "source" : {
                "type" : "BUILD_OUTPUT",
                "path" : "nginx/bin-noperl",
                "files" : [
                    "nginx"
                ]
            },
            "destination" : {
                "path" : "/usr/sbin/"
            }
        },
        {
            "source" : {
                "type" : "ARCADIA",
                "path" : "nginx/bin/debians/nginx"
            },
            "destination" : {
                "path" : "/etc/"
            }
        }
    ]
}
