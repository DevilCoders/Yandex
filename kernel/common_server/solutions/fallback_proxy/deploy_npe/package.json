{
    "meta": {
        "name": "fintech-fallback-proxy",
        "version": "{revision}",
        "maintainer": "Amir Mennibaev <mennibaev@yandex-team.ru>"
    },
    "build": {
        "server": {
            "targets": [
                "kernel/common_server/solutions/fallback_proxy/src"
            ]
        }
    },
    "data": [
        {
          "source": {
            "type": "BUILD_OUTPUT",
            "build_key": "server",
            "path": "kernel/common_server/solutions/fallback_proxy/src"
          },
          "destination": {
            "path": "/bin"
          }
        },
        {
          "source": {
            "type": "ARCADIA",
            "path": "kernel/common_server/solutions/fallback_proxy/configs/fallback-npe.conf"
          },
          "destination": {
            "path": "/conf/server.conf",
          }
        },
        {
          "source": {
            "type": "RELATIVE",
            "path": "cloud/env"
          },
          "destination": {
            "path": "/conf/env",
          }
        },
        {
          "source": {
              "type": "RELATIVE",
              "path": "cloud/Dockerfile"
          },
          "destination": {
              "path": "/Dockerfile"
          }
        },
        {
          "source": {
            "type": "ARCADIA",
            "path": "fintech/ci/service_build_scripts/common/root.crt"
          },
          "destination": {
            "path": "/postgresql_root.crt",
          }
        },
        {
          "source": {
            "type": "ARCADIA",
            "path": "kernel/common_server/solutions/fallback_proxy/migrations"
          },
          "destination": {
            "path": "/migrations"
          }
        },
        {
          "source": {
            "type": "ARCADIA",
            "path": "kernel/common_server/migrations/common_migrations"
          },
          "destination": {
            "path": "/migrations_common"
          }
        },
        {
          "source": {
            "type": "SANDBOX_RESOURCE",
            "id"  : 2668970521
          },
          "destination": {
            "path": "/sources.list"
          }
        }
    ]
}
