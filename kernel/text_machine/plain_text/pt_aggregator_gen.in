Using {
    CppNamespace: "::NTextMachine::NCore"
}

Machine {
    Name: "PlainTextMaxAggregatorArray"
    Domain: "AggregatorArray"

    Unit { Name: "Count" }

    Unit { Name: "SumF" }
    Unit { Name: "MaxF" }

    Unit { Name: "SumW" }
    Unit { Name: "MaxW" }

    Unit { Name: "SumWF" }
    Unit { Name: "SumW2F" }
    Unit { Name: "MaxWF" }

    ## Max
    ##
    Unit { Name: "MaxFCopy" }
    Unit { Name: "MaxWFCopy" }
    Unit { Name: "MaxWFNormMaxW" }
    Unit { Name: "MaxWFNormSumW" }

    ## Avg
    ##
    Unit { Name: "SumFNormCount" }
    Unit { Name: "SumWFNormSumW" }
    Unit { Name: "SumW2FNormSumW" }
}

Machine {
    Name: "PlainTextMinAggregatorArray"
    Domain: "AggregatorArray"

    Unit { Name: "Count" }

    Unit { Name: "SumF" }
    Unit { Name: "MinF" }

    Unit { Name: "SumW" }
    Unit { Name: "MinW" }
    Unit { Name: "MaxW" }

    Unit { Name: "SumWF" }
    Unit { Name: "SumW2F" }
    Unit { Name: "MinWF" }

    ## Min
    ##
    Unit { Name: "MinFCopy" }
    Unit { Name: "MinWFCopy" }
    Unit { Name: "MinWFNormMaxW" }
    Unit { Name: "MinWFNormSumW" }

    ## Avg
    ##
    Unit { Name: "SumFNormCount" }
    Unit { Name: "SumWFNormSumW" }
    Unit { Name: "SumW2FNormSumW" }
}

Machine {
    Name: "PlainTextAggregator"
    Domain: "Aggregator"
    Unit { Name: "All"; Arg { Name: "PlainTextMaxAggregatorArray" } }
    Unit { Name: "TopHeap"; FamilyArg: "10"; Arg { Name: "PlainTextMinAggregatorArray" } }
}

