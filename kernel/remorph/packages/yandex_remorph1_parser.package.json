{
    "meta": {
        "name": "yandex-remorph1-parser",
        "maintainer": "Nikita Slyusarev <nslus@yandex-team.ru>",
        "description": "Yandex Remorph library\nRemorph is a word-based morphology-aware regular expression library.\n.\nThis package contains Remorph parser tool.",
        "version": "1.11.1-{revision}",
        "depends": ["${shlibs:Depends}", "${misc:Depends}"],
        "replaces": ["libyandex-remorph-dev", "yandex-remorph1-dev (<= 1.2.0-1)"],
        "breaks": ["libyandex-remorph-dev", "yandex-remorph1-dev (<= 1.2.0-1)"],
        "homepage": "https://wiki.yandex-team.ru/PoiskovajaPlatforma/Lingvistika/Remorph"
    },
    "build": {
        "targets": [
            "tools/remorphparser"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "tools/remorphparser/remorphparser"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        }
    ]
}
