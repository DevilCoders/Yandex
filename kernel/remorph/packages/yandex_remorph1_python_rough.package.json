{
    "meta": {
        "name": "yandex-remorph1-python-rough",
        "maintainer": "Nikita Slyusarev <nslus@yandex-team.ru>",
        "description": "Yandex Remorph library\nRemorph is a word-based morphology-aware regular expression library.\n.\nThis package contains python bindings (wikidate variant).",
        "version": "1.11.1-{revision}",
        "depends": ["python (>=2.7)", "${shlibs:Depends}", "${misc:Depends}"],
        "homepage": "https://wiki.yandex-team.ru/PoiskovajaPlatforma/Lingvistika/Remorph"
    },
    "build": {
        "targets": [
            "kernel/remorph/bindings/python_rough"
        ],
        "flags": [
            {
                "name": "USE_ARCADIA_PYTHON",
                "value": "no"
            }
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "kernel/remorph/bindings/python_rough",
                "files": [
                    "libyandex_remorph_python_rough.so",
                    "yandex_remorph_python_rough.py"
                ]
            },
            "destination": {
                "path": "/usr/lib/python2.7/dist-packages/"
            }
        }
    ]
}
