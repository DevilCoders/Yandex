{
    "meta": {
        "name": "yandex-remorph1",
        "maintainer": "Nikita Slyusarev <nslus@yandex-team.ru>",
        "description": "Yandex Remorph library\nRemorph is a word-based morphology-aware regular expression library.\n.\nThis package contains Remorph runtime library.",
        "version": "1.11.1-{revision}",
        "depends": ["${shlibs:Depends}", "${misc:Depends}"],
        "replaces": ["libyandex-remorph"],
        "breaks": ["libyandex-remorph"],
        "homepage": "https://wiki.yandex-team.ru/PoiskovajaPlatforma/Lingvistika/Remorph"
    },
    "build": {
        "targets": [
            "kernel/remorph/api/dynamic"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "kernel/remorph/api/dynamic",
                "files": [
                    "libyandex-remorph.so.*"
                ]
            },
            "destination": {
                "path": "/usr/lib/"
            }
        }
    ]
}
