{
    "meta": {
        "name": "yandex-remorph1-dev",
        "maintainer": "Nikita Slyusarev <nslus@yandex-team.ru>",
        "description": "Yandex Remorph library\nRemorph is a word-based morphology-aware regular expression library.\n.\nThis package contains:\nAPI headers to use with the library\nremorphc - remorph rules compiler\ngztcompiler - gazetteer dictionaries compiler\nand some other misc files.",
        "version": "1.11.1-{revision}",
        "depends": ["yandex-remorph1 (>=1.11)", "${shlibs:Depends}", "${misc:Depends}"],
        "replaces": ["libyandex-remorph-dev"],
        "breaks": ["libyandex-remorph-dev"],
        "homepage": "https://wiki.yandex-team.ru/PoiskovajaPlatforma/Lingvistika/Remorph"
    },
    "build": {
        "targets": [
            "kernel/remorph/api/dynamic",
            "tools/remorphc",
            "dict/gazetteer/compiler"
        ]
    },
    "data": [
        {
            "source": {
                "type": "SYMLINK"
            },
            "destination": {
                "path": "/usr/lib/libyandex-remorph.so",
                "target": "libyandex-remorph.so.1"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "tools/remorphc/remorphc"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "dict/gazetteer/compiler/gztcompiler"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "kernel/remorph/api",
                "files": [
                    "remorph.h",
                    "remorph_wrap.h"
                ]
            },
            "destination": {
                "path": "/usr/include/yandex/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "kernel/remorph/cascade/cascade.gztproto"
            },
            "destination": {
                "path": "/usr/share/yandex/remorph/"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "test_native",
                "files": [
                    "remorph-test-dev.sh",
                    "info.cpp"
                ]
            },
            "destination": {
                "path": "/usr/share/yandex/remorph/test/"
            }
        }
    ]
}
