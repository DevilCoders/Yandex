import "kernel/gazetteer/proto/base.proto";

message TSpecialWordProto: TArticle {
    enum EType {
        STOPWORD = 0;
        STOPWORD_EXCEPTION = 1;
        MARKER = 2;
        REPLACED_WORD = 3;
    }
    required EType Type = 2;
    optional string Marker = 3 [default = "__marker__"];
    optional string Replacement = 4 [default = ""];
}
