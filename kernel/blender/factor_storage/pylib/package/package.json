{
    "meta": {
        "name": "yandex-blender-factor-storage",
        "maintainer": "zhevnerchuk <zhevnerchuk@yandex-team.ru>",
        "version": "r{revision}"
    },
    "build": {
        "targets": [
            "kernel/blender/factor_storage/pylib/so",
            "kernel/blender/factor_storage/pylib",
            "kernel/blender/factor_storage/protos"
        ],
        "flags": [
            {
                "name": "USE_SYSTEM_PYTHON",
                "value": "2.7"
            }
        ],
        "add-result": [ ".py" ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "kernel/blender/factor_storage/protos",
                "files": [ "storage_pb2.py" ]
            },
            "destination": {
                "path": "/kernel/blender/factor_storage/protos/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "kernel/blender/factor_storage/pylib",
                "files": [ "serialization.py" ]
            },
            "destination": {
                "path": "/kernel/blender/factor_storage/pylib/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "kernel/blender/factor_storage/pylib/so/compression",
                "files": [ "compression.so" ]
            },
            "destination": {
                "path": "/kernel/blender/factor_storage/pylib/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "kernel/blender/factor_storage/pylib/package/kernel",
                "files": [ "*" ]
            },
            "destination": {
                "path": "/kernel/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "kernel/blender/factor_storage/pylib/package",
                "files": [ "setup.py" ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
