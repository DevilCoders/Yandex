{
    "meta": {
        "version": "{sandbox_task_id}.{branch}",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
        "name": "yc-nbs-systemd",
        "description": "Package with nbs init script for Ycloud (Kikimr)"
    },
    "data": [
         {
            "source": {
                "path": "etc/rsyslog.d/10-nbs.conf",
                "type": "RELATIVE"
            },
            "destination": {
                "path": "/etc/rsyslog.d/"
            }
        },
        {
            "source": {
                "path": "etc/logrotate.d/yc-nbs-systemd",
                "type": "RELATIVE"
            },
            "destination": {
                "path": "/etc/logrotate.d/"
            }
        },
        {
            "source": {
                "path": "init/nbs.service",
                "type": "RELATIVE"
            },
            "destination": {
                "path": "/usr/lib/systemd/system/"
            }
        },
        {
            "source": {
                "path": "usr/bin/yc-nbs-init",
                "type": "RELATIVE"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        },
        {
            "source": {
                "path": "usr/bin/yc-nbs2-init",
                "type": "RELATIVE"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        },
        {
            "source": {
                "path": "usr/bin/yc-nbs-restart",
                "type": "RELATIVE"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        }
    ],
    "build": {
        "add-result": [],
        "flags": [],
        "targets": []
    }
}
