{
    "meta": {
        "name": "yc-nbs-ci-tools",
        "version": "latest",
        "description": "Yandex Cloud NBS CI tools",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
        "pre-depends": [
        ]
    },
    "build": {
        "targets": [
            "cloud/blockstore/tools/ci/check_emptiness_test",
            "cloud/blockstore/tools/ci/checkpoint_validation_test",
            "cloud/blockstore/tools/ci/corruption_test_suite",
            "cloud/blockstore/tools/ci/deploy_packages",
            "cloud/blockstore/tools/ci/fio_performance_test_suite",
            "cloud/blockstore/tools/ci/migration_test",
            "cloud/blockstore/tools/ci/run_ya_make",
            "cloud/blockstore/tools/ci/run_ya_package",
            "cloud/blockstore/tools/ci/sign_packages",
            "cloud/blockstore/tools/ci/worst_case_read_performance_test_suite",
            "cloud/blockstore/tools/testing/eternal-tests/checkpoint-validator",
            "cloud/blockstore/tools/testing/verify-test",
            "cloud/disk_manager/test/acceptance",
            "cloud/filestore/tests/build-arcadia-test"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "ycp-config.yaml"
            },
            "destination": {
                "path": "/ycp-config.yaml"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/testing/eternal-tests/checkpoint-validator/bin/checkpoint-validator",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/checkpoint-validator"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/checkpoint_validation_test/yc-nbs-ci-checkpoint-validation-test",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-checkpoint-validation-test"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/testing/verify-test/verify-test",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/verify-test"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/corruption_test_suite/yc-nbs-ci-corruption-test-suite",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-corruption-test-suite"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/deploy_packages/yc-nbs-ci-deploy-packages",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-deploy-packages"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/fio_performance_test_suite/yc-nbs-ci-fio-performance-test-suite",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-fio-performance-test-suite"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/worst_case_read_performance_test_suite/yc-nbs-ci-worst-case-read-performance-test-suite",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-worst-case-read-performance-test-suite"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/run_ya_make/yc-nbs-ci-run-ya-make",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-run-ya-make"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/run_ya_package/yc-nbs-ci-run-ya-package",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-run-ya-package"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/sign_packages/yc-nbs-ci-sign-packages",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-sign-packages"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/migration_test/yc-nbs-ci-migration-test",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-migration-test"
            }
        },
        {
            "source": {
                "path": "cloud/blockstore/tools/ci/check_emptiness_test/yc-nbs-ci-check-nrd-disk-emptiness-test",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nbs-ci-check-nrd-disk-emptiness-test"
            }
        },
        {
            "source": {
                "path": "cloud/disk_manager/test/acceptance/acceptance-test",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/acceptance-test"
            }
        },
        {
            "source": {
                "path": "cloud/disk_manager/test/acceptance/test_runner/yc-disk-manager-ci-acceptance-test-suite",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-disk-manager-ci-acceptance-test"
            }
        },
        {
            "source": {
                "path": "cloud/filestore/tests/build-arcadia-test/yc-nfs-ci-build-arcadia-test",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/yc-nfs-ci-build-arcadia-test"
            }
        }
    ]
}
