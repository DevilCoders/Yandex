{
    "meta": {
        "name": "nbsclient",
        "version": "0.{revision}+{branch}.{sandbox_task_id}",
        "description": "Yandex NBS Python Library",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
        "pre-depends": [
        ]
    },
    "build": {},
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/blockstore/public/sdk/python/client",
                "files": [
                    "*.py"
                ]
            },
            "destination": {
                "path": "/cloud/blockstore/public/sdk/python/client/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/blockstore/public/sdk/python/protos",
                "files": [
                    "*.py"
                ]
            },
            "destination": {
                "path": "/cloud/blockstore/public/sdk/python/protos/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/blockstore/public/api/grpc",
                "files": [
                    "*.proto"
                ]
            },
            "destination": {
                "path": "/cloud/blockstore/public/api/grpc/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/blockstore/public/api/protos",
                "files": [
                    "*.proto"
                ]
            },
            "destination": {
                "path": "/cloud/blockstore/public/api/protos/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/storage/core/protos",
                "files": [
                    "*.proto"
                ]
            },
            "destination": {
                "path": "/cloud/storage/core/protos/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "contrib/libs/googleapis-common-protos/google/api",
                "files": [
                    "*.proto"
                ]
            },
            "destination": {
                "path": "/google/api/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "library/cpp/lwtrace/protos",
                "files": [
                    "*.proto"
                ]
            },
            "destination": {
                "path": "/library/cpp/lwtrace/protos/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/blockstore/packages/nbsclient-python",
                "files": [
                    "setup.py",
                    "MANIFEST.in"
                ]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "INLINE",
                "content": "0.{revision}+{branch}.{sandbox_task_id}"
            },
            "destination": {
                "path": "/VERSION"
            }
        }
    ]
}
