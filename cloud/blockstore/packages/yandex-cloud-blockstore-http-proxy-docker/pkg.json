{
    "meta": {
        "name": "yandex-cloud-blockstore-http-proxy",
        "version": "{sandbox_task_id}.{branch}",
        "description": "Yandex Cloud BlockStore HTTP Proxy",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
	"pre-depends": [
        ]
    },
    "build": {
        "targets": [
            "cloud/blockstore/tools/http_proxy"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
	{
            "source": {
                "path": "cloud/blockstore/tools/http_proxy",
                "type": "BUILD_OUTPUT",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
