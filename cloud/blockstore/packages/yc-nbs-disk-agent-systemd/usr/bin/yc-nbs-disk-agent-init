#!/bin/bash -e

nbs_restarts_count_file=/var/log/nbs-disk-agent/nbs-disk-agent.restart
source /Berkanavt/nbs-server/cfg/nbs_disk_agent.cfg

nbs_command="/usr/bin/blockstore-disk-agent ${nbs_arg}"

if [ -v nbs_breakpad_arg ]; then
    nbs_breakpad_command="/usr/bin/yc-storage-breakpad-launcher ${nbs_breakpad_arg}"
    nbs_command="${nbs_breakpad_command} -- ${nbs_command}"
fi

exec $nbs_command
