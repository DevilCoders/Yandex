/var/log/nbs-disk-agent/nbs-disk-agent.log
{
        rotate 10
        create 644 syslog adm
        missingok
        daily
        ifempty
        compress
        sharedscripts
        postrotate
                /bin/systemctl kill -s HUP --kill-who=main rsyslog.service >/dev/null 2>&1 || true
        endscript
}
