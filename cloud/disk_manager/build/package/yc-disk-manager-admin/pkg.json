{
    "meta": {
        "name": "yc-disk-manager-admin",
        "description": "Yandex Cloud Disk Manager - admin console",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
        "version": "{sandbox_task_id}.{branch}"
    },
    "build": {
        "targets": [
            "cloud/disk_manager/cmd/yc-disk-manager-admin"
        ]
