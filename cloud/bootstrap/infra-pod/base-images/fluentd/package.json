{
    "meta": {
        "name": "fluentd",
        "maintainer": "Rashit Azizbaev <syndicut@yandex-team.ru>",
        "description": "Fluentd image for infra-pod base on https://bb.yandex-team.ru/projects/CLOUD/repos/cloud-go/browse/api/fluent/docker",
        "version": "latest"
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "fluent.conf"
            },
            "destination": {
                "path": "/fluent.conf"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "entrypoint.sh"
            },
            "destination": {
                "path": "/entrypoint.sh"
            }
        }
    ]
}
