{
  "meta": {
    "name": "yc-kubectl-saltformula",
    "maintainer": "Yc EngInfra Team <yc_selfhost@yandex-team.ru>",
    "description": "kubectl plugin for salt-operator managed resources",
    "version": "0.0.1-{revision}"
  },
  "build": {
    "targets": [
      "cloud/bootstrap/k8s/salt-operator/cmd/kubectl-saltformula"
    ]
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "cloud/bootstrap/k8s/salt-operator/cmd/kubectl-saltformula"
      },
      "destination": {
        "path": "/usr/bin"
      }
    }
  ],
  "params": {
    "format": "debian"
  }
}
