{
    "meta": {
        "name": "spinnaker-tools",
        "maintainer": "Yc EngInfra Team <yc_selfhost@yandex-team.ru>",
        "description": "Docker image with tools for exporting spinnaker pipelines",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "vendor/cuelang.org/go/cmd/cue"
        ]
    },
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/bootstrap/k8s/spinnaker/Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "vendor/cuelang.org/go/cmd/cue/cue"
            },
            "destination": {
                "path": "/cue"
            }
        }
    ]
}
