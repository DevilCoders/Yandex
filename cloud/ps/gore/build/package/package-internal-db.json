{
    "meta": {
        "name": "gore",
        "maintainer": "megaeee <megaeee@yandex-team.ru>",
        "description": "Yandex.Cloud internal duty system",
        "version": "0.1.52-debian"
    },
    "build": {
        "targets": [
            "cloud/ps/gore"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile-internal"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/ps/gore/cmd/ultragore/ultragore"
            },
            "destination": {
                "path": "/gore"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ps/gore/ya.make"
            },
            "destination": {
                "path": "/ya.make",
                "attributes": {
                    "mode": {
                        "value": "+x"
                    }
                }
            }
        }
    ]
}


