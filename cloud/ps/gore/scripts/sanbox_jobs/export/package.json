{
    "meta": {
        "name": "gore-export-task",
        "maintainer": "Andrew Gein <andgein@yandex-team.ru>",
        "description": "GoRe export task",
        "version": "1"
    },
    "build": {
        "targets": [
            "cloud/ps/gore/scripts/sanbox_jobs/export"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/ps/gore/scripts/sanbox_jobs/export/sandbox_exporter_task"
            },
            "destination": {
                "path": "/sandbox_exporter_task"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ps/gore/scripts/sanbox_jobs/export/ya.make"
            },
            "destination": {
                "path": "/ya.make",
                "attributes": {
                    "mode": {
                        "value": "+x"
                    }
                }
            }
        }
    ]
}
