{
    "meta": {
        "name": "connman",
        "maintainer": "Data transfer developers <data-transfer-dev@yandex-team.ru>",
        "description": "Connection Manager",
        "version": "{svn_revision}"
    },
    "build": {
        "targets": [
            "cloud/dataplatform/connman/cmd/connmand",
            "cloud/dataplatform/connman/cmd/connmanctl"
        ],
        "build_type": "release",
        "flags": [
            {
                "name": "DATA_TRANSFER_DISABLE_CGO",
                "value": "true"
            }
        ],
        "target-platforms": [
            "DEFAULT-LINUX-X86_64"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/dataplatform/connman",
                "files": [
                    "$CONFIG_FILE"
                ]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/dataplatform/connman/cmd/connmanctl",
                "files": [
                    "connmanctl"
                ]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/dataplatform/connman/cmd/connmand",
                "files": [
                    "connmand"
                ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
