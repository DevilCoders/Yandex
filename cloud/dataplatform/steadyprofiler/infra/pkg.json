{
    "meta": {
      "name": "profiler-archive",
      "maintainer": "Andrei Tserakhau<tserakhau@yandex-team.ru>",
      "description": "Continius profiler app",
      "version": "{svn_revision}"
    },
    "build": {
      "targets": [
        "transfer_manager/go/internal/profiler/server"
      ],
      "build_type": "release"
    },
    "data": [
      {
        "source": {
          "type": "ARCADIA",
          "path": "transfer_manager/go/internal/profiler/Dockerfile"
        },
        "destination": {
          "path": "/"
        }
      },
      {
        "source": {
          "type": "ARCADIA",
          "path": "transfer_manager/go/internal/profiler/ui",
          "files": [
            "package.json",
            "package-lock.json"
          ]
        },
        "destination": {
          "path": "/"
        }
      },
      {
        "source": {
          "type": "ARCADIA",
          "path": "transfer_manager/go/internal/profiler/ui/src"
        },
        "destination": {
          "path": "/src"
        }
      },
      {
        "source": {
          "type": "ARCADIA",
          "path": "transfer_manager/go/internal/profiler/deploy"
        },
        "destination": {
          "path": "/deploy"
        }
      },
      {
        "source": {
          "type": "BUILD_OUTPUT",
          "path": "transfer_manager/go/internal/profiler/server",
          "files": [
            "server"
          ]
        },
        "destination": {
          "path": "/"
        }
      }
    ]
  }
