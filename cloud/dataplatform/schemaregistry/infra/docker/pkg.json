{
  "meta": {
    "name": "schemaregistry",
    "maintainer": "Data transfer developers <data-transfer-dev@yandex-team.ru>",
    "description": "Schema Registry",
    "version": "{svn_revision}"
  },
  "build": {
    "targets": [
      "cloud/dataplatform/schemaregistry"
    ],
    "build_type": "release",
    "flags": [
      {
        "name": "DATA_TRANSFER_DISABLE_CGO",
        "value": "true"
      }
    ],
    "target-platforms": [
      "DEFAULT-LINUX-X86_64"
    ]
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "cloud/dataplatform/schemaregistry",
        "files": [
          "*.yaml"
        ]
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "cloud/dataplatform/schemaregistry",
        "files": [
          "schemaregistry"
        ]
      },
      "destination": {
        "path": "/"
      }
    }
  ]
}
