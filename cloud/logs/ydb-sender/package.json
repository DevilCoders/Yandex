{
    "meta": {
        "name": "logs-ydb-sender",
        "maintainer": "bgleb <bgleb@yandex-team.ru>",
        "description": "ydb-sender",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/logs/ydb-sender"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/logs/ydb-sender/ydb-sender"
            },
            "destination": {
                "path": "/ydb-sender"
            }
        }
    ]
}
