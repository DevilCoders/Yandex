{%- from 'slb-adapter/map.jinja' import port_map, proxy_protocol with context %}
{% for slb_port, upstream_port in port_map.iteritems() %}
server {
    listen         {{ slb_port }};
    listen         [::]:{{ slb_port }};
    proxy_protocol {{ proxy_protocol }};
    proxy_pass     {{ upstream }}:{{ upstream_port }};
}
{% endfor %}
