/Berkanavt/kikimr/logs/kikimr.log

{
        rotate 168
        maxage 7
        create 644 syslog adm
        missingok
        ifempty
        compress
        sharedscripts
        postrotate
                /bin/systemctl kill -s HUP --kill-who=main rsyslog.service >/dev/null 2>&1 || true
        endscript
}
