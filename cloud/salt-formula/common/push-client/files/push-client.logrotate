/var/log/statbox/*.log {
    weekly
    rotate 4
    notifempty
    missingok
    compress
    delaycompress
    dateext
    sharedscripts
        postrotate
                ( test -x /etc/init.d/statbox-push-client && /etc/init.d/statbox-push-client reload > /dev/null ) || exit 0
        endscript
}
