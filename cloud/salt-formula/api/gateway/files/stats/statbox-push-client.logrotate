/var/log/statbox/*.log {
    daily
    maxsize 1024M
    rotate 168
    notifempty
    missingok
    compress
    delaycompress
    dateext
    dateformat -%Y%m%d-%s
    sharedscripts
        postrotate
                ( test -x /etc/init.d/statbox-push-client && /etc/init.d/statbox-push-client reload > /dev/null ) || exit 0
        endscript
}
