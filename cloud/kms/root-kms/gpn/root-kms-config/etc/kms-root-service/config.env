APPLICATION_YAML="/etc/kms/application.yaml"
APPLICATION_LOG_DIR="/var/log/kms"
JAVA_OPTS="-XX:-UseContainerSupport \
    -Djavax.net.ssl.trustStore=/etc/kms/gpn-ca-certs \
    -Djavax.net.ssl.trustStorePassword=changeit \
    -Djava.net.preferIPv6Addresses=true \
    -Dfile.encoding=UTF-8 \
    -Xlog:gc*:file=/var/log/kms/gc.log::filecount=10,filesize=100M"
MASTER_KEY_PATH="/etc/kms/master-key-config.yaml"
KEYS_PATH="/etc/kms/keys-config.yaml"
