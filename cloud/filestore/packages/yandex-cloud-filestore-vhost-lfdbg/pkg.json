{
    "meta": {
        "name": "yandex-cloud-filestore-vhost",
        "version": "{sandbox_task_id}.{branch}",
        "description": "Yandex Cloud FileStore vhost",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
        "pre-depends": [
        ]
    },
    "build": {
        "targets": [
            "cloud/filestore/vhost/lfdbg"
        ],
        "flags": [
            {
                "name": "HARDENING",
                "value": "yes"
            },
            {
                "name": "PROFILE_MEMORY_ALLOCATIONS",
                "value": "yes"
            },
            {
                "name": "CFLAGS",
                "value": "-ggdb -fno-omit-frame-pointer"
            }
        ]
    },
    "data": [
        {
            "source": {
                "path": "cloud/filestore/vhost/lfdbg",
                "type": "BUILD_OUTPUT",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/usr/bin/"
            }
        }
    ]
}
