#!/bin/sh

set -e
case "$1" in
    remove|deconfigure)
        systemctl stop nfs-breakpad-sender.service || :
        rm -rf "/var/tmp/breakpad-nfs" || :
        ;;

    failed-upgrade|upgrade)
        ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

exit 0
