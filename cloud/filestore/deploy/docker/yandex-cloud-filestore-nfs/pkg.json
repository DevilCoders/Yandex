{
    "meta": {
        "name": "yandex-cloud-filestore-nfs",
        "version": "{revision}.{branch}.{sandbox_task_id}",
        "description": "Yandex Cloud FileStore NFS server",
        "maintainer": "Cloud NBS <nbs-dev@yandex-team.ru>",
        "pre-depends": []
    },
    "build": {
        "targets": [
            "cloud/filestore/gateway/nfs/server"
        ],
        "flags": [
            {
                "name": "HARDENING",
                "value": "yes"
            },
            {
                "name": "CFLAGS",
                "value": "-ggdb -fno-omit-frame-pointer"
            }
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "path": "cloud/filestore/gateway/nfs/server",
                "type": "BUILD_OUTPUT",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
