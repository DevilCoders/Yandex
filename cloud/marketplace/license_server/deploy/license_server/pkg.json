{
    "meta": {
        "name": "marketplace-license-server",
        "maintainer": "Ya Cloud Marketplace team <yc-mkt-dev@yandex-team.ru>",
        "description": "License Server implementation",
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/cloud/marketplace/license_server",
        "noconffiles_all": false,
        "version": "{branch}-{revision}"
    },
    "build": {
        "build_type": "release",
        "targets": [
            "cloud/marketplace/license_server/cmd/license_server",
            "cloud/marketplace/license_server/dev/lsc"
        ],
        "target-platforms": [
            "DEFAULT-LINUX-X86_64"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/marketplace/license_server/cmd/license_server"
            },
            "destination": {
                "path": "/cmd/license_server"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/marketplace/license_server/dev/lsc"
            },
            "destination": {
                "path": "/dev/lsc"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "INLINE",
                "content": "{branch}-{revision}"
            },
            "destination": {
                "path": "/version.txt"
            }
        }
    ]
}
