{
    "meta": {
        "name": "yc-mkt-bootstrap",
        "maintainer": "Alex Ilichev <rewle@yandex-team.ru>",
        "description": "YC Marketplace Bootstrap",
        "version": "2019.06-{revision}.{branch}",
        "build-depends": ["debhelper (>= 7)"]
    },
    "build": {
        "build_1": {
            "targets": ["cloud/marketplace/misc"]
        }
    },
    "data": [{
        "source": {
            "type": "ARCADIA",
            "path": "cloud/marketplace/misc/juggler"
        },
        "destination": {
            "path": "/juggler"
        }
    }, {
        "source": {
            "type": "ARCADIA",
            "path": "cloud/marketplace/misc/utils"
        },
        "destination": {
            "path": "/utils"
        }
    }]
}
