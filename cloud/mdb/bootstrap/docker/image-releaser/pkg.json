{
  "meta": {
    "name": "image-releaser",
    "maintainer": "Yandex Cloud",
    "description": "MDB Image Releaser",
    "version": "{revision}"
  },
  "build": {
    "targets": [
      "cloud/mdb/image-releaser/cmd/image-releaser"
    ]
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "cloud/mdb/image-releaser/cmd/image-releaser/image-releaser"
      },
      "destination": {
        "path": "/build/image-releaser"
      }
    }
  ]
}
