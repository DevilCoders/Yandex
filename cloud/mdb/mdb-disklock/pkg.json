{
  "meta": {
    "name": "mdb-disklock",
    "maintainer": "MDB <mdb@yandex-team.ru>",
    "description": "MDB disklock tool",
    "version": "1.{svn_revision}",
    "homepage": "https://wiki.yandex-team.ru/MDB/"
  },
  "build": {
    "targets": [
      "cloud/mdb/mdb-disklock/cmd/mdb-disklock"
    ],
    "target-platforms": [
      "linux"
    ]
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "cloud/mdb/mdb-disklock/cmd/mdb-disklock/mdb-disklock"
      },
      "destination": {
        "path": "/opt/yandex/mdb-disklock/mdb-disklock"
      }
    }
  ]
}
