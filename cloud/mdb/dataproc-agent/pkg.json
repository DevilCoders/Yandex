{
    "meta": {
        "name": "dataproc-agent",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Service for report health status from dataproc clusters and starting jobs",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/dataproc-agent/cmd/dataproc-agent"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/dataproc-agent/cmd/dataproc-agent/dataproc-agent"
            },
            "destination": {
                "path": "/opt/yandex/dataproc-agent/bin/dataproc-agent"
            }
        }
    ]
}
