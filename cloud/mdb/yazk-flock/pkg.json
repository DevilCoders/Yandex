{
    "meta": {
        "name": "yazk-flock",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "zk-flock like util built on top of library/ylock",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/yazk-flock"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/yazk-flock/yazk-flock"
            },
            "destination": {
                "path": "/usr/bin/yazk-flock"
            }
        }
    ]
}
