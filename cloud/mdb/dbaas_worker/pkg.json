{
    "meta": {
        "name": "dbaas-worker",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Worker",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "dbaas_worker": {
            "targets": [
                "cloud/mdb/dbaas_worker/bin"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "dbaas_worker",
                "path": "cloud/mdb/dbaas_worker/bin/dbaas-worker"
            },
            "destination": {
                "path": "/opt/yandex/dbaas-worker/bin/dbaas-worker"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "dbaas-worker.conf"
            },
            "destination": {
                "path": "/etc/supervisor/conf.d/dbaas-worker.conf"
            }
        }
    ]
}
