{
    "meta": {
        "name": "mdb-juggler-tools",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "version": "1.{svn_revision}",
        "description": "jclient wrapper",
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/{package_root}"
    },
    "build": {
        "targets": [
            "metrika/admin/python/scripts/jrun/"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "metrika/admin/python/scripts/jrun/jrun"
            },
            "destination": {
                "path": "/usr/local/bin/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "metrika/admin/packages/config-juggler-metrika-yandex/jrun_bash_completion"
            },
            "destination": {
                "path": "/etc/bash_completion.d/jrun"
            }
        }
    ]
}
