{
    "meta": {
        "name": "mdb-pgmigrate",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Build of yandex-pgmigrate",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "pgmigrate": {
            "targets": [
                "contrib/python/yandex-pgmigrate/bin"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "pgmigrate",
                "path": "contrib/python/yandex-pgmigrate/bin/pgmigrate"
            },
            "destination": {
                "path": "/opt/yandex/pgmigrate/bin/pgmigrate"
            }
        }
    ]
}
