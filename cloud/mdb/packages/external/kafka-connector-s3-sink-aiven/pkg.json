{
    "meta": {
        "name": "kafka-connector-s3-sink",
        "maintainer": "Yandex Data Processing Team <mdb-dataproc@yandex-team.ru>",
        "description": "S3-Sink connector for Apache Kafka from Aiven",
        "version": "1-2.12.0",
        "depends": ["openjdk-11-jre", "kafka"],
        "noconffiles_all": true,
        "homepage": "https://github.com/aiven/s3-connector-for-apache-kafka"
    },
    "data": [
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": 2656539968,
                "path": "aiven-s3.jar",
                "untar": false
            },
            "destination": {
                "path": "/etc/kafka-connect/plugins/",
                "mode": {
                    "value": "777",
                    "recursive": true
                },
                "owner": {
                    "value": "root",
                    "recursive": true
                }
            }
        }
    ]
}
