{
    "meta": {
        "name": "mdb-blackbox-sso",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Single SignOn helper with blackbox support",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mdb-blackbox-sso/cmd/mdb-blackbox-sso"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-blackbox-sso/cmd/mdb-blackbox-sso/mdb-blackbox-sso"
            },
            "destination": {
                "path": "/opt/yandex/mdb-blackbox-sso/bin/mdb-blackbox-sso"
            }
        }
    ]
}
