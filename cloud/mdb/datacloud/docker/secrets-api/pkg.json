{
    "meta": {
        "name": "secrets-api",
        "maintainer": "Data Cloud",
        "description": "Secrets API",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/mdb-secrets/cmd/mdb-secrets"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/mdb-secrets/cmd/mdb-secrets/mdb-secrets"
            },
            "destination": {
                "path": "/build/mdb-secrets"
            }
        }
    ]
}
