{
    "meta": {
        "name": "py-api",
        "maintainer": "MDB",
        "description": "(PY) Internal API",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/dbaas-internal-api-image/uwsgi/ya.make"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/dbaas-internal-api-image/uwsgi/internal-api.wsgi"
            },
            "destination": {
                "path": "/build/uwsgi_internal_api"
            }
        }
    ]
}
