{
    "meta": {
        "name": "temporaldb-migrations",
        "maintainer": "Data Cloud",
        "description": "Migrations for Temporal PostgreSQL db",
        "version": "0.{revision}"
    },
    "build": {
        "targets": [
          "cloud/mdb/internal/python/pg_create_users/bin"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
          "source": {
            "type": "BUILD_OUTPUT",
            "path": "cloud/mdb/internal/python/pg_create_users/bin/create_users"
          },
          "destination": {
            "path": "/build/create_users"
          }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/datacloud/docker/temporaldb-migrations/migrate.sh"
            },
            "destination": {
                "path": "/migrate.sh"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}
