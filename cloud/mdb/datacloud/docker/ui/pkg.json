{
    "meta": {
        "name": "ui",
        "maintainer": "Data Cloud",
        "description": "UI",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/ui/bin"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
      {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/ui/bin/manage/manage"
            },
            "destination": {
                "path": "/build/manage"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/ui/bin/http/http"
            },
            "destination": {
                "path": "/build/http"
            }
        }
    ]
}
