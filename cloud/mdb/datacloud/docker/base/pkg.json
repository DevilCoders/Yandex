{
    "meta": {
        "name": "datacloud-base",
        "maintainer": "Data Cloud",
        "description": "Base Image",
        "version": "latest"
    },
    "build": {
        "targets": [
            "vendor/github.com/fullstorydev/grpcurl/cmd/grpcurl",
            "vendor/github.com/go-delve/delve/cmd/dlv"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "bootstrap.sh"
            },
            "destination": {
                "path": "/bootstrap.sh"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "vendor/github.com/fullstorydev/grpcurl/cmd/grpcurl/grpcurl"
            },
            "destination": {
                "path": "/build/grpcurl"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "vendor/github.com/go-delve/delve/cmd/dlv/dlv"
            },
            "destination": {
                "path": "/build/dlv"
            }
        }
    ]
}
