{
    "meta": {
        "name": "vpcdb-migrations",
        "maintainer": "Data Cloud",
        "description": "Migrations for vpcdb",
        "version": "1"
    },
    "build": {
        "targets": [
            "cloud/mdb/internal/python/pg_create_users/bin",
            "contrib/python/yandex-pgmigrate/bin"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/internal/python/pg_create_users/bin/create_users"
            },
            "destination": {
                "path": "/build/create_users"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "contrib/python/yandex-pgmigrate/bin/pgmigrate"
            },
            "destination": {
                "path": "/build/pgmigrate"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/datacloud/docker/vpcdb-migrations/migrate.sh"
            },
            "destination": {
                "path": "/migrate.sh"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/mdb-vpc/db/migrations"
            },
            "destination": {
                "path": "/build/vpcdb/migrations"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/mdb-vpc/db/vpcdb.sql"
            },
            "destination": {
                "path": "/build/vpcdb/vpcdb.sql"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}
