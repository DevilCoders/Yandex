{
    "meta": {
        "name": "mdb-mlock",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Mlock service",
        "version": "1.{svn_revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
        "targets": [
            "cloud/mdb/mlock/cmd/mdb-mlock",
            "cloud/mdb/mlock/cmd/mdb-mlock-cli"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/mlock/cmd/mdb-mlock/mdb-mlock"
            },
            "destination": {
                "path": "/build/mdb-mlock"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/mlock/cmd/mdb-mlock-cli/mdb-mlock-cli"
            },
            "destination": {
                "path": "/build/mdb-mlock-cli"
            }
        },
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": 2257039314,
                "untar": false
            },
            "destination": {
                "path": "/build/grpc_health_probe",
                "attributes": {
                    "mode": {
                        "value": "+x"
                    }
                }
            }
        }
    ]
}
