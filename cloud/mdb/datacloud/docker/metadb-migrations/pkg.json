{
    "meta": {
        "name": "metadb-migrations",
        "maintainer": "Data Cloud",
        "description": "Migrations for metadb",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/internal/python/populate_table/bin",
            "cloud/mdb/tools/vr_gen2/bin",
            "cloud/mdb/tools/vr_gen",
            "cloud/mdb/internal/python/pg_create_users/bin",
            "contrib/python/yandex-pgmigrate/bin",
            "cloud/mdb/tools/pgmigrator/cmd/pgmigrator"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/tools/vr_gen2/bin/vr_gen2"
            },
            "destination": {
                "path": "/build/vr_gen2"
            }
        },
            {
                "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/tools/vr_gen/vr_gen"
            },
            "destination": {
                "path": "/build/vr_gen"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/internal/python/populate_table/bin/populate_table"
            },
            "destination": {
                "path": "/build/populate_table"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/internal/python/pg_create_users/bin/create_users"
            },
            "destination": {
                "path": "/build/create_users"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "contrib/python/yandex-pgmigrate/bin/pgmigrate"
            },
            "destination": {
                "path": "/build/pgmigrate"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/tools/pgmigrator/cmd/pgmigrator/pgmigrator"
            },
            "destination": {
                "path": "/build/pgmigrator"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/datacloud/docker/metadb-migrations/resources"
            },
            "destination": {
                "path": "/resources"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/datacloud/docker/metadb-migrations/migrate.sh"
            },
            "destination": {
                "path": "/migrate.sh"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/dbaas_metadb/migrations"
            },
            "destination": {
                "path": "/build/dbaas_metadb/migrations"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/dbaas_metadb/metadb.sql"
            },
            "destination": {
                "path": "/build/dbaas_metadb/metadb.sql"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/dbaas_metadb/head/grants"
            },
            "destination": {
                "path": "/build/dbaas_metadb/head/grants"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}
