{
    "meta": {
        "name": "dbaas-worker",
        "maintainer": "Data Cloud",
        "description": "DBaaS Worker",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/dbaas_worker/bin"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/dbaas_worker/bin/dbaas-worker"
            },
            "destination": {
                "path": "/build/dbaas-worker"
            }
        }
    ]
}
