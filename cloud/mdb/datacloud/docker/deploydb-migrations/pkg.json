{
    "meta": {
        "name": "deploydb-migrations",
        "maintainer": "Data Cloud",
        "description": "Migrations for deploydb",
        "version": "1"
    },
    "build": {
        "targets": [
            "cloud/mdb/deploy/api/cmd/mdb-deploy-registrar",
            "cloud/mdb/internal/python/pg_create_users/bin",
            "contrib/python/yandex-pgmigrate/bin"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/deploy/api/cmd/mdb-deploy-registrar/mdb-deploy-registrar"
            },
            "destination": {
                "path": "/build/mdb-deploy-registrar"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/internal/python/pg_create_users/bin/create_users"
            },
            "destination": {
                "path": "/build/create_users"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "contrib/python/yandex-pgmigrate/bin/pgmigrate"
            },
            "destination": {
                "path": "/build/pgmigrate"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/datacloud/docker/deploydb-migrations/migrate.sh"
            },
            "destination": {
                "path": "/migrate.sh"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/deploydb/migrations"
            },
            "destination": {
                "path": "/build/deploydb/migrations"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/deploydb/deploy.sql"
            },
            "destination": {
                "path": "/build/deploydb/deploy.sql"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/deploydb/grants"
            },
            "destination": {
                "path": "/build/deploydb/grants"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/deploydb/code"
            },
            "destination": {
                "path": "/build/deploydb/code"
            }
        }
    ]
}
