{
    "meta": {
        "name": "release-promote",
        "maintainer": "DoubleCloud",
        "description": "Release Promote",
        "version": "latest"
    },
    "build": {
        "targets": [
            "cloud/mdb/release/cmd/alerts_checker"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "bootstrap.sh"
            },
            "destination": {
                "path": "/bootstrap.sh"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/release/cmd/alerts_checker/alerts_checker"
            },
            "destination": {
                "path": "/build/alerts_checker"
            }
        }
    ]
}
