{
    "meta": {
        "name": "deploy-cleaner",
        "maintainer": "Double Cloud",
        "description": "DeployDB cleaner",
        "version": "{svn_revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/deploy/api/cmd/mdb-deploy-cleaner"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/deploy/api/cmd/mdb-deploy-cleaner/mdb-deploy-cleaner"
            },
            "destination": {
                "path": "/mdb-deploy-cleaner"
            }
        }
    ]
}
