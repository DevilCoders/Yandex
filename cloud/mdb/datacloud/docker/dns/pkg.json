{
    "meta": {
        "name": "dns",
        "maintainer": "Data Cloud",
        "description": "DNS",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "cloud/mdb/mdb-dns/cmd/mdb-dns"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/mdb/mdb-dns/cmd/mdb-dns/mdb-dns"
            },
            "destination": {
                "path": "/build/mdb-dns"
            }
        }
    ]
}
