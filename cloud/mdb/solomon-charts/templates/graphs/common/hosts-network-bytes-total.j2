{
  "id": "<< id_prefix >>-hosts-network-bytes-total",
  "projectId": "<< project_id >>",
  "name": "<< human_prefix >> Hosts: Network usage, [bytes/s] (total)",
  "description": "",
  "parameters": [
    {
      "name": "project",
      "value": "<< project_id >>"
    },
{% if dbm %}
    {
      "name": "service",
      "value": "dom0"
    },
    {
      "name": "cluster",
      "value": "internal-mdb_dom0"
    },
    {
      "name": "cid",
      "value": "*"
    }
{% else %}
    {
      "name": "service",
      "value": "*"
    },
    {
      "name": "cluster",
      "value": "*"
    },
    {
      "name": "shard",
      "value": "*"
    }
{% endif %}
  ],
  "elements": [
    {
      "title": "total",
      "type": "EXPRESSION",
{% if dbm %}
      "expression": "sum({name='/porto/net_rx_bytes', host=by_cid_container} + {name='/porto/net_tx_bytes', host=by_cid_container}) by name"
{% else %}
      "expression": "sum({name='/System/network/eth0/bytes_recv', node=by_host} + {name='/System/network/eth0/bytes_sent', node=by_host}) by name"
{% endif %}
    }
  ],
  "graphMode": "GRAPH",
  "secondaryGraphMode": "BARS",
  "min": "",
  "max": "",
  "normalize": false,
  "colorScheme": "AUTO",
  "stack": false,
  "aggr": "AVG",
  "interpolate": "LINEAR",
  "scale": "NATURAL",
  "numberFormat": "3|auto",
  "green": "",
  "yellow": "",
  "red": "",
  "violet": "",
  "greenValue": "",
  "yellowValue": "",
  "redValue": "",
  "violetValue": "",
  "sortByLabel": "",
  "asc": false,
  "limit": "",
  "overLinesTransform": "NONE",
  "percentiles": "",
  "transform": "NONE",
  "movingWindow": "",
  "movingPercentile": "",
  "downsampling": "AUTO",
  "downsamplingAggr": "DEFAULT",
  "grid": "",
  "maxPoints": 0,
  "hideNoData": false,
  "ignoreInf": false
}
