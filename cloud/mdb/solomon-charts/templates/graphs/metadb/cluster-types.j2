{
  "id": "<< id_prefix >>-metadb-cluster-types",
  "projectId": "<< project_id >>",
  "name": "Clusters by types",
  "description": "",
  "parameters": [
    {
      "name": "cluster",
      "value": "<< metadb.get("cluster") >>"
    },
    {
      "name": "service",
      "value": "<< metadb.get("service") >>"
    }
  ],
  "elements": [
{% for engine in engines %}
    {
      "title": "<< g.db_ctxs[engine].name >>",
      "type": "SELECTORS",
      "selectors": [
        {
          "name": "host",
          "value": "by_node"
        },
        {
          "name": "node",
          "value": "primary"
        },
        {
          "name": "sensor",
          "value": "dbaas_metadb_clusters_status_<< engine >>_cluster/RUNNING"
        },
        {
          "name": "dc",
          "value": "by_node"
        },
        {
          "name": "shard",
          "value": "*"
        }
      ],
      "expression": "",
      "stack": "",
      "color": "",
      "yaxis": "LEFT",
      "transform": "NONE"
    }
    {% if not loop.last %}
    ,  
    {% endif %}
{% endfor %}
  ],
  "graphMode": "GRAPH",
  "secondaryGraphMode": "PIE",
  "min": "",
  "max": "",
  "normalize": false,
  "colorScheme": "AUTO",
  "stack": true,
  "aggr": "AVG",
  "interpolate": "LINEAR",
  "scale": "NATURAL",
  "numberFormat": "1.123",
  "green": "",
  "yellow": "",
  "red": "",
  "violet": "",
  "greenValue": "",
  "yellowValue": "",
  "redValue": "",
  "violetValue": "",
  "sortByLabel": "",
  "asc": true,
  "limit": "",
  "overLinesTransform": "NONE",
  "percentiles": "",
  "filter": "NONE",
  "filterBy": "AVG",
  "filterLimit": "",
  "transform": "NONE",
  "movingWindow": "",
  "movingPercentile": "",
  "downsampling": "AUTO",
  "downsamplingAggr": "AVG",
  "grid": "",
  "maxPoints": 0,
  "hideNoData": false,
  "ignoreInf": false
}
