{
  "id": "<< id_prefix >>-<< grpc_interface >>-ok-dc",
  "projectId": "<< project_id >>",
  "name": "<< human_prefix >> << (grpc_interface|resolve)["human_name"] >> OK by DC",
  "descriptiodn": "",
  "parameters": [
    {
      "name": "project",
      "value": "<< project_id >>"
    },
    {
      "name": "cluster",
      "value": "<< (grpc_interface|resolve)["cluster"] >>"
    },
    {
      "name": "service",
      "value": "<< (grpc_interface|resolve)["service"] >>"
    }
  ],
  "elements": [
    {% for dc, val in  dc_lst.items() %}
    { "title": "<< dc >>",
      "area": true,
      "type": "EXPRESSION",
      "yaxis": "LEFT",
      "expression": "group_lines('sum', { dc=\"<< dc >>\", name=\"<< "OK"|grpc_status_to_signal((grpc_interface|resolve)["prefix"], (grpc_interface|resolve)["postfix"]) >>\",  host=\"<< (grpc_interface|resolve)["host"] >>\"})",
      "color": "<< val.color >>"
    }
    {% if not loop.last %}
    ,  
    {% endif %}
    {% endfor %}
  ],
  "graphMode": "GRAPH",
  "secondaryGraphMode": "PIE",
