{
  "id": "mdb-internal-api-2xx-rate",
  "projectId": "{{projectId}}",
  "name": "MDB Internal API: HTTP 2xx replies by DC  ",
  "description": "MDB Internal API: HTTP 2xx response rate",
  "parameters": [
    {
      "name": "project",
      "value": "<< project_id >>"
    },
    {
      "name": "cluster",
      "value": "<< py_int_api.get("cluster") >>"
    },
    {
      "name": "service",
      "value": "<< py_int_api.get("service") >>"
    }
  ],
  "elements": [
{% for dc, val in  dc_lst.items() %}
    {
      "title": "<< dc >>",
      "type": "EXPRESSION",
      "selectors": [],
      "expression": "sum({host=\"<< py_int_api.get("host_mask") >>\", sensor=\"int_api_dbaas_internal_api_count_2xx_dmmm_rate\", dc=\"<< dc >>\"}) by sensor",
      "stack": "",
      "color": "<< val["color"] >>",
      "yaxis": "LEFT",
      "transform": "NONE"
    }
      {% if not loop.last %}
      ,
      {% endif %}
{% endfor %}
  ],
  "graphMode": "GRAPH",
  "secondaryGraphMode": "PIE",
  "min": "",
  "max": "",
  "normalize": false,
  "colorScheme": "AUTO",
  "stack": true,
  "aggr": "AVG",
  "interpolate": "LINEAR",
  "scale": "NATURAL",
  "numberFormat": "",
  "green": "",
  "yellow": "",
  "red": "",
  "violet": "",
  "greenValue": "",
  "yellowValue": "",
  "redValue": "",
  "violetValue": "",
  "sortByLabel": "",
  "asc": false,
  "limit": "",
  "overLinesTransform": "NONE",
  "percentiles": "",
  "ignoreInf": false,
  "filter": "NONE",
  "filterBy": "AVG",
  "filterLimit": "",
  "transform": "NONE",
  "movingWindow": "",
  "movingPercentile": "",
  "downsampling": "AUTO",
  "downsamplingAggr": "AVG",
  "grid": "",
  "maxPoints": 0,
  "hideNoData": false
}
