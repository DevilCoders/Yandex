{
  "id": "mdb-mlock",
  "projectId": "{{projectId}}",
  "name": "MDB Mlock",
  "description": "",
  "heightMultiplier": 1,
  "parameters": [
    {
      "name": "cluster",
      "value": "mdb_mlock_test"
    },
    {
      "name": "service",
      "value": "mdb|yandexcloud_dbaas"
    },
    {
      "name": "project",
      "value": "{{projectId}}"
    }
  ],
  "rows": [
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "GRPC Codes",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&sensor=mdb_mlock_grpc_server_handled_total_grpc_code_*_ammv_rate&graph=auto&stack=0&l.host=by_node&scale=log",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "CPU",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.sensor=mdb_*_process_cpu_seconds_total_ammv_rate&graph=auto&stack=0&l.&dc=%3F%3F%3F",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Memory Heap Inuse",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.sensor=mdb_*_go_memstats_heap_inuse_bytes_ammv&graph=auto&stack=0&l.&dc=%3F%3F%3F",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "GRPC Messages",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&sensor=mdb_mlock_grpc_server_msg_*_ammv_rate&graph=auto&stack=0&l.host=by_node&scale=log",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Goroutines & Threads",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.sensor=mdb_*_go_threads_ammv|mdb_*_go_goroutines_ammv&graph=auto&stack=0&l.&dc=%3F%3F%3F",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Memory Stack Inuse",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.sensor=mdb_*_go_memstats_stack_inuse_bytes_ammv&graph=auto&stack=0&l.&dc=%3F%3F%3F",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    }
  ]
}
