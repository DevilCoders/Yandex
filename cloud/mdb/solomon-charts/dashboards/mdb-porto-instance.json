{
  "id": "mdb-porto-instance",
  "projectId": "{{projectId}}",
  "name": "MDB Porto Instance",
  "description": "TODO: link to doc",
  "heightMultiplier": 1,
  "parameters": [
    {
      "name": "project",
      "value": "internal-mdb"
    },
    {
      "name": "cluster",
      "value": "internal-mdb_dom0"
    },
    {
      "name": "service",
      "value": "dom0"
    },
    {
      "name": "container",
      "value": "*"
    },
    {
      "name": "host",
      "value": "by_cid_container"
    },
    {
      "name": "dc",
      "value": "by_cid_container"
    }
  ],
  "rows": [
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "CPU",
          "subtitle": "CPU Usage",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&container={{container}}&host={{host}}&dc={{dc}}&graph={{prefix}}mdb-porto-instance-cpu",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Disk",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&container={{container}}&host={{host}}&dc={{dc}}&graph={{prefix}}mdb-porto-instance-disk",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Memory",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&container={{container}}&host={{host}}&dc={{dc}}&graph={{prefix}}mdb-porto-instance-memory",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
