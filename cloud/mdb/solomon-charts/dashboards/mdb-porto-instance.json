        {
          "type": "IFRAME",
          "title": "Network bandwidth",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&container={{container}}&host={{host}}&dc={{dc}}&cid={{cid}}&graph={{prefix}}mdb-porto-instance-network",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Network packets",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&container={{container}}&host={{host}}&dc={{dc}}&cid={{cid}}&graph={{prefix}}mdb-porto-instance-network-packets",
