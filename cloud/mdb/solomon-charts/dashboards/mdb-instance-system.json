{
  "id": "mdb-instance-system",
  "projectId": "{{projectId}}",
  "name": "MDB System metrics",
  "description": "TODO: link to documentation",
  "heightMultiplier": 1,
  "parameters": [
    {
      "name": "cluster",
      "value": "*"
    },
    {
      "name": "host",
      "value": "*"
    },
    {
      "name": "service",
      "value": "*"
    }
  ],
  "rows": [
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "CPU",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host={{host}}&dc=by_host&graph={{prefix}}mdb-instance-system-cpu",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Memory",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host={{host}}&dc=by_host&graph={{prefix}}mdb-instance-system-memory",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Disk bytes",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host={{host}}&dc=by_host&graph={{prefix}}mdb-instance-system-disk-bytes",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Disk IOPS",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host={{host}}&dc=by_host&graph={{prefix}}mdb-instance-system-disk-iops",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Network bytes",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host={{host}}&dc=by_host&graph={{prefix}}mdb-instance-system-network",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Network packets",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host={{host}}&dc=by_host&graph={{prefix}}mdb-instance-system-network-packets",
          "markdown": "",
          "rowspan": 0,
