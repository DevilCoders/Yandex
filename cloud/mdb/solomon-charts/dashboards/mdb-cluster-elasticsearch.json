{
  "id": "mdb-cluster-elasticsearch",
  "projectId": "{{projectId}}",
  "name": "MDB Elasticsearch",
  "description": "",
  "heightMultiplier": 1,
  "parameters": [
    {
      "name": "cluster",
      "value": "*"
    },
    {
      "name": "service",
      "value": "mdb|yandexcloud_dbaas"
    },
    {
      "name": "project",
      "value": "{{projectId}}"
    }
  ],
  "rows": [
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "JVM Heap Usage, [bytes]",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.name=elasticsearch_jvm_mem_heap_used_in_bytes&dc=by_host&host=*&graph=auto&stack=0",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "JVM Heap Usage, [percent]",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.name=elasticsearch_jvm_mem_heap_used_percent&dc=by_host&host=*&graph=auto&stack=0",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Indices Total Store Size, [bytes]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&l.name=elasticsearch_indices_store_size_in_bytes&dc=by_host&host=*&graph=auto&stack=0&l.shard=datanode",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Search Queries, [count]",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.name=elasticsearch_indices_search_query_total_inc&dc=by_host&host=*&graph=auto&stack=0&l.shard=datanode",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Index Queries, [count]",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.name=elasticsearch_indices_indexing_index_total_inc&dc=by_host&host=*&graph=auto&stack=0&l.shard=datanode",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Merges, [count]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&l.name=elasticsearch_indices_merges_total_inc&dc=by_host&host=*&graph=auto&stack=0&l.shard=datanode",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Client Connections, [count]",
          "subtitle": "",
          "url": "?cluster={{cluster}}&project={{project}}&service={{service}}&l.name=elasticsearch_http_total_opened_inc&dc=total&host=*&graph=auto&stack=0&aggr=max&&numberFormat=0%7Cauto",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Is Alive",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&l.name=elasticsearch_is_alive&l.host=*&l.dc=by_host&graph=auto&graphMode=bars&aggr=last",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    }
  ]
}
