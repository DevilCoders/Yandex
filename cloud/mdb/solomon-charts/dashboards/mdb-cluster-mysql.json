{
  "id": "mdb-cluster-mysql",
  "projectId": "{{projectId}}",
  "name": "MDB MySQL",
  "description": "",
  "heightMultiplier": 1,
  "parameters": [
    {
      "name": "cluster",
      "value": "*"
    },
    {
      "name": "service",
      "value": "mdb|yandexcloud_dbaas"
    },
    {
      "name": "project",
      "value": "{{projectId}}"
    }
  ],"rows": [
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Threads, [count]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=total&dc=total&host=total&graph=auto&l.sensor=mysql_Threads_running%7Cmysql_Threads_connected%7Cmysql_Threads_cached&stack=false",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Is Alive, [boolean]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&l.node=by_host&l.sensor=mysql_is_alive&l.dc=by_host&graph=auto",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host=*&dc=by_host&sensor=mysql_is_alive&graph=auto&graphMode=bars&aggr=last",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Queries, [count]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&host=*&sensor=mysql_Queries_rate&dc=by_host&graph=auto&stack=false",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Slow queries, [count]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&host=*&sensor=mysql_Slow_queries_rate&dc=by_host&graph=auto&stack=false",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Is Primary, [boolean]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&node=by_host&host=*&dc=by_host&sensor=mysql_is_primary&graph=auto&graphMode=bars&aggr=last&hideNoData=true",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    },
    {
      "panels": [
        {
          "type": "IFRAME",
          "title": "Disk capacity on primary, [bytes]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&host=by_node&node=primary&sensor=disk-used_bytes_mysql_data%7Cdisk-used_bytes_mysql_relaylogs%7Cdisk-used_bytes_mysql_binlogs%7Cdisk-free_bytes_mysql&graph=auto&hideNoData=true",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        },
        {
          "type": "IFRAME",
          "title": "Replication lag, [seconds]",
          "subtitle": "",
          "url": "?project={{project}}&cluster={{cluster}}&service={{service}}&host=*&sensor=mysql_replication_lag&graph=auto&stack=false&dc=by_host&hideNoData=true",
          "markdown": "",
          "rowspan": 0,
          "colspan": 0
        }
      ]
    }
  ]
}
