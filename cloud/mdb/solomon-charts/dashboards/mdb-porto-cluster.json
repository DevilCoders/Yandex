{
  "id": "mdb-porto-cluster",
  "projectId": "{{projectId}}",
  "name": "MDB Porto Cluster",
  "description": "TODO: link to doc",
  "heightMultiplier": 1,
  "parameters": [
    {
      "name": "project",
      "value": "internal-mdb"
    },
    {
      "name": "cluster",
      "value": "internal-mdb_dom0"
    },
    {
      "name": "service",
      "value": "dom0"
    },
    {
      "name": "container",
      "value": "by_cid"
    },
    {
      "name": "host",
      "value": "by_cid"
    },
    {
      "name": "dc",
      "value": "by_cid"
    },
    {
      "name": "cid",
      "value": "*"
    }
  ],
  "rows": [
