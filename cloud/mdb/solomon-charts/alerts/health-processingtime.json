{
    "id": "<< env >>-processingtime",
    "projectId": "{{projectId}}",
    "name": "[<< svc >>][<< env >>] mdb-health processing aggregate loop",
    "version": 1,
    "createdBy": "isiv",
    "notificationChannels": ["<< notification_channel >>"],
    "type": {
        "expression": {
            "program": "{{alert_program}}",
            "checkExpression": "is_red"
        }
    },
    "annotations": {
        "trafficLight.color": "{{expression.trafficColor}}",
        "host": "<< health_cluster_name >>_vhost",
        "service": "aggregate_time",
        "description": "mdb-health average aggregation times: {{expression.avg_times}}"
    },

    "periodMillis": 60000, "delaySeconds": 30,
    "description": "When processing aggregate loop take a lot time"
}
