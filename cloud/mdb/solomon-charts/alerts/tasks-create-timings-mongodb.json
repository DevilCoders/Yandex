{
    "id": "mdb-<< env >>-tasks-create-timings-mongodb",
    "projectId": "{{projectId}}",
    "name": " [<< svc >>][<< env >>] MongoDB creating timings",
    "createdBy": "asalimonov",
    "notificationChannels": ["<< notification_channel >>"],
    "type": {
        "expression": {
            "program": "{{alert_program}}",
            "checkExpression": "is_red"
        }
    },
  "annotations": {
    "description": "Creating of MongoDB cluster 20min+: {{expression.over20}}",
    "timings": "20min+ - {{expression.over20}}, 45min+ - {{expression.over45}}",
    "trafficLight.color": "{{expression.trafficColor}}",
    "host": "mongodb-<< worker_host >>",
    "service": "dbaas_worker_slow_new_cluster_task_monogdb"
  },
    "periodMillis": 10800000,
    "delaySeconds": 1800,
    "description": ""
}
