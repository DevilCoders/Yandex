{
"id": "mdb-<< env >>-tasks-create-timings-dataproc",
    "projectId": "{{projectId}}",
    "name": " [<< svc >>][<< env >>] Data Proc creating timings",
    "createdBy": "asalimonov",
    "notificationChannels": ["<< notification_channel >>"],
    "type": {
        "expression": {
            "program": "{{alert_program}}",
            "checkExpression": "is_red"
        }
    },
    "annotations": {
      "description": "Creating of Data Proc cluster requires too much time. 20min+: {{expression.over20}}",
      "timings": "20min+: {{expression.over20}}",
      "trafficLight.color": "{{expression.trafficColor}}",
      "host": "hadoop-<< worker_host >>",
      "service": "dbaas_worker_slow_new_cluster_task_hadoop"
},
    "periodMillis": 10800000,
    "delaySeconds": 1800,
    "description": ""
}
