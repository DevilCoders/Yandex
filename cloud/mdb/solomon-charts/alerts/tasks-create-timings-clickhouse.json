{
    "id": "mdb-<< env >>-tasks-create-timings-clickhouse",
    "projectId": "{{projectId}}",
    "name": " [<< svc >>][<< env >>] ClickHouse creating timings",
    "createdBy": "asalimonov",
    "notificationChannels": ["<< notification_channel >>"],
    "type": {
        "expression": {
            "program": "{{alert_program}}",
            "checkExpression": "is_red"
        }
    },
    "annotations": {
      "description": "Creating of ClickHouse cluster 30min+: {{expression.over30}}",
      "timings": "30min+ - {{expression.over30}}, 45min+ - {{expression.over45}}",
      "trafficLight.color": "{{expression.trafficColor}}",
      "host": "clickhouse-<< worker_host >>",
      "service": "dbaas_worker_slow_new_cluster_task_clickhouse"
    },
    "periodMillis": 10800000,
    "delaySeconds": 1800,
    "description": ""
}
