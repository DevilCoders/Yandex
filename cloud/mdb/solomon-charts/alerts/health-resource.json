{
    "id": "<< env >>-resource",
    "projectId": "{{projectId}}",
    "name": "[<< svc >>][<< env >>] mdb-health goroutines or threads are above limit",
    "version": 1,
    "createdBy": "isiv",
    "notificationChannels": ["<< notification_channel >>"],
    "type": {
        "expression": {
            "program": "{{alert_program}}",
            "checkExpression": "is_red"
        }
    },
    "annotations": {
        "trafficLight.color": "{{expression.trafficColor}}",
        "host": "<< health_cluster_name >>_vhost",
        "service": "threads_over_limit",
        "description": "goroutines/threads: {{expression.goroutines_out}}/{{expression.threads_out}}, goroutines/threads over limit: {{expression.goroutines_over}}/{{expression.threads_over}}"
    },

    "periodMillis": 60000, "delaySeconds": 30,
    "description": "When goroutines or threads more than limit"
}
