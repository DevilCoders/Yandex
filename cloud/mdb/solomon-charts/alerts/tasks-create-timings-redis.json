{
    "id": "mdb-<< env >>-tasks-create-timings-redis",
    "projectId": "{{projectId}}",
    "name": "[<< svc >>][<< env >>] Redis creating timings",
    "createdBy": "asalimonov",
    "notificationChannels": ["<< notification_channel >>"],
    "type": {
        "expression": {
            "program": "{{alert_program}}",
            "checkExpression": "is_red"
        }
    },
    "annotations": {
        "description": "Creating of Redis cluster 20min+: {{expression.over20}}",
        "timings": "20min+ - {{expression.over20}}, 45min+ - {{expression.over45}}",
        "trafficLight.color": "{{expression.trafficColor}}",
        "host": "postgresql-<< worker_host >>",
        "service": "dbaas_worker_slow_new_cluster_task_redis"
    },
    "periodMillis": 10800000,
    "delaySeconds": 1800,
    "description": ""
}
