{
    "meta": {
        "name": "yandex-lazy-trim",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Small utility for ssd disks throtted trimming",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/lazy_trim/src"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/lazy_trim/src/lazy_trim"
            },
            "destination": {
                "path": "/opt/yandex/lazy_trim/bin/lazy_trim"
            }
        }
    ]
}
