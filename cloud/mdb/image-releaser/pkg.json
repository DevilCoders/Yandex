{
    "meta": {
        "name": "mdb-image-releaser",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Checks images tests and releases them",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
        "bin": {
            "targets": [
                "cloud/mdb/image-releaser/cmd/image-releaser/"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/image-releaser/cmd/image-releaser/image-releaser"
            },
            "destination": {
                "path": "/opt/yandex/mdb-image-releaser/bin/mdb-image-releaser"
            }
        }
    ]
}
