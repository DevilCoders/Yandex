{
    "meta": {
        "name": "mdb-mlock",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Mlock service",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mlock/cmd/mdb-mlock"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mlock/cmd/mdb-mlock/mdb-mlock"
            },
            "destination": {
                "path": "/opt/yandex/mdb-mlock/mdb-mlock"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/mlock/cmd/mdb-mlock/mdb-mlock.service"
            },
            "destination": {
                "path": "/lib/systemd/system/mdb-mlock.service"
            }
        }
    ]
}
