{
    "meta": {
        "name": "mdb-mlock-cli",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Mlock cli",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mlock/cmd/mdb-mlock-cli"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mlock/cmd/mdb-mlock-cli/mdb-mlock-cli"
            },
            "destination": {
                "path": "/usr/local/bin/mlock"
            }
        }
    ]
}
