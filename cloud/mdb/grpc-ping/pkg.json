{
    "meta": {
        "name": "grpc-ping",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Binary for ping mdb grpc services",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/grpc-ping"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/grpc-ping/grpc-ping"
            },
            "destination": {
                "path": "/opt/yandex/grpc-ping/grpc-ping"
            }
        }
    ]
}
