{
    "meta": {
        "name": "mdb-search-producer",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Service that send clusters meta from dbaas_metadb to LogBroker",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mdb-search-producer/cmd/mdb-search-producer",
                "cloud/mdb/mdb-search-producer/cmd/mdb-search-queue-documents-age"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-search-producer/cmd/mdb-search-producer/mdb-search-producer"
            },
            "destination": {
                "path": "/opt/yandex/mdb-search-producer/bin/mdb-search-producer"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-search-producer/cmd/mdb-search-queue-documents-age/mdb-search-queue-documents-age"
            },
            "destination": {
                "path": "/opt/yandex/mdb-search-producer/bin/mdb-search-queue-documents-age"
            }
        }
    ]
}
