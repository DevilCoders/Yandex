<% set cid_tags = 'ctype=' ~ cid %>
<% set dom0_tags = '' %>
<% set domu_tags = '' %>
<% if tier %>
<% set dom0_tags = dom0_tags ~ ';tier=' ~ tier %>
<% set domu_tags = domu_tags ~ ';tier=' ~ tier %>
<% else %>
<% set dom0_tags = dom0_tags ~ ';tier=primary,replica' %>
<% set domu_tags = domu_tags ~ ';tier=primary,replica' %>
<% endif %>
<% if geo %>
<% set dom0_tags = dom0_tags ~ ';geo=' ~ geo %>
<% set domu_tags = domu_tags ~ ';geo=' ~ geo %>
<% endif %>
{
    "title": "MySQL cluster: << cid >>",
    "type": "panel",
    "charts": [
        {
            "id": "cpu",
            "type": "graphic",
            "row": 1,
            "col": 1,
            "yAxis": [
                {
                    "minValue": 0,
                    "maxValue": 24
                },
                {
                    "minValue": 0,
                    "maxValue": 24
                }
            ],
            "signals": [
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-cpu_usage_cores_tmmv",
                    "title": "Used",
                    "yAxis": 0
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-cpu_guarantee_cores_tmmv",
                    "title": "Guaranteed",
                    "stacked": false,
                    "yAxis": 1
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-cpu_limit_cores_tmmv",
                    "title": "Limit",
                    "stacked": false,
                    "yAxis": 1
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-cpu_wait_cores_tmmv",
                    "title": "Wait",
                    "yAxis": 0
                }
            ],
            "consts": [],
            "title": "CPU (Cores)",
            "stacked": true,
            "normalize": true
        },
        {
            "id": "mem",
            "type": "graphic",
            "row": 1,
            "col": 3,
            "yAxis": [
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-memory_usage_gb_tmmv",
                    "title": "Used"
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-memory_guarantee_gb_tmmv",
                    "title": "Guaranteed"
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-memory_limit_gb_tmmv",
                    "title": "Limit"
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-anon_usage_gb_tmmv",
                    "title": "Anon used"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Memory (GB)",
            "stacked": false,
            "normalize": true
        },
        {
            "id": "io",
            "type": "graphic",
            "row": 1,
            "col": 5,
            "yAxis": [
                {
                    "minValue": 0,
                    "maxValue": 1000000000
                },
                {
                    "minValue": 0,
                    "maxValue": 1000000000
                }
            ],
            "signals": [
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-io_read_fs_bytes_tmmv",
                    "title": "read",
                    "normalizable": true,
                    "yAxis": 0
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-io_write_fs_bytes_tmmv",
                    "title": "write",
                    "normalizable": true,
                    "yAxis": 0
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-io_limit_bytes_tmmv",
                    "title": "limit",
                    "stacked": false,
                    "normalizable": true,
                    "yAxis": 1
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "I/O (B/s)",
            "stacked": true,
            "normalize": true
        },
        {
            "id": "net",
            "type": "graphic",
            "row": 3,
            "col": 1,
            "yAxis": [
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "sum(portoinst-net_rx_mb_summ, portoinst-net_tx_mb_summ)",
                    "title": "Used"
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-net_guarantee_mb_summ",
                    "title": "Guaranteed"
                },
                {
                    "tag": "itype=mdbdom0;<< cid_tags >><< dom0_tags >>",
                    "host": "CON",
                    "name": "portoinst-net_limit_mb_summ",
                    "title": "Limit"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Network (MB/s)",
            "stacked": false,
            "normalize": true
        },
        {
            "id": "threads",
            "type": "graphic",
            "row": 3,
            "col": 3,
            "yAxis": [
                {},
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdb;<< cid_tags >><< domu_tags >>",
                    "host": "CON",
                    "name": "push-mysql_Threads_connected_vmmv",
                    "yAxis": 0,
                    "title": "connected"
                },
                {
                    "tag": "itype=mdb;<< cid_tags >><< domu_tags >>",
                    "host": "CON",
                    "name": "push-mysql_Threads_running_vmmv",
                    "yAxis": 0,
                    "title": "running"
                },
                {
                    "tag": "itype=mdb;<< cid_tags >><< domu_tags >>",
                    "host": "CON",
                    "name": "push-mysql_Threads_cached_vmmv",
                    "yAxis": 0,
                    "title": "cached"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Threads",
            "stacked": false,
            "normalize": true
        },
        {
            "id": "queries",
            "type": "graphic",
            "row": 3,
            "col": 5,
            "yAxis": [
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdb;<< cid_tags >><< domu_tags >>",
                    "host": "CON",
                    "name": "push-mysql_Queries_rate_vmmv",
                    "title": "queries rate"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Queries",
            "stacked": false,
            "normalize": true
        },
        {
            "id": "slow_queries",
            "type": "graphic",
            "row": 5,
            "col": 1,
            "yAxis": [
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdb;<< cid_tags >><< domu_tags >>",
                    "host": "CON",
                    "name": "push-mysql_Slow_queries_rate_vmmv",
                    "title": "slow queries rate"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Slow Queries",
            "stacked": false,
            "normalize": true
        },
        {
            "id": "size",
            "type": "graphic",
            "row": 5,
            "col": 3,
            "minValue": 0,
            "yAxis": [
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdb;<< cid_tags >>",
                    "host": "CON",
                    "name": "push-disk-used_bytes_/var/lib/mysql_tmmx",
                    "title": "used_space (bytes)"
                },
                {
                    "tag": "itype=mdb;<< cid_tags >>",
                    "host": "CON",
                    "name": "push-disk-free_bytes_/var/lib/mysql_tmmx",
                    "title": "free_space (bytes)"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Database size",
            "stacked": true,
            "normalize": true
        },
        {
            "id": "replication_lag",
            "type": "graphic",
            "row": 5,
            "col": 5,
            "minValue": 0,
            "yAxis": [
                {}
            ],
            "signals": [
                {
                    "tag": "itype=mdb;tier=replica;<< cid_tags >>",
                    "host": "CON",
                    "name": "push-mysql_replication_lag_vmmv"
                }
            ],
            "consts": [],
            "range": 1500,
            "title": "Replication lag (on replicas)",
            "stacked": false,
            "normalize": true
        },
        {
            "id": "free_space",
            "type": "graphic",
            "row": 7,
            "col": 1,
            "minValue": 0,
            "yAxis": [
                {}
            ],
            "signals": [
                <% for host in list_hosts(itype='mdb', ctype=cid) %>
                {
                    "tag": "itype=mdb;<< cid_tags >>",
                    "host": "<< host >>",
                    "name": "mul(push-disk-total_bytes_/var/lib/mysql_tmmx, 0.05)",
                    "title": "RO Threshold << host >>"
                },
                <% endfor %>
                <% for host in list_hosts(itype='mdb', ctype=cid) %>
                {
                    "tag": "itype=mdb;<< cid_tags >>",
                    "host": "<< host >>",
                    "name": "push-disk-free_bytes_/var/lib/mysql_tmmx",
                    "title": "Free space << host >>"
                },
                <% endfor %>
            ],
            "consts": [],
            "range": 1500,
            "title": "Free space (bytes)",
            "stacked": false,
            "normalize": false
        }
    ]
}
