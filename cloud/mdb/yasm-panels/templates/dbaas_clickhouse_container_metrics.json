
<% set dom0_tags_list = ['itype=mdbdom0'] %>
<% set domu_tags_list = ['itype=mdbclickhouse'] %>


<% if prj %>
<% do dom0_tags_list.append('prj=' ~ prj) %>
<% do domu_tags_list.append('prj=' ~ prj) %>
<% endif %>


<% if tier %>
<% do dom0_tags_list.append('tier=' ~ tier) %>
<% do domu_tags_list.append('tier=' ~ tier) %>
<% else %>
<% do domu_tags_list.append('tier=replica') %>
<% endif %>

<% set dom0_tags = ';'.join(dom0_tags_list)%>
<% set domu_tags = ';'.join(domu_tags_list)%>



{
