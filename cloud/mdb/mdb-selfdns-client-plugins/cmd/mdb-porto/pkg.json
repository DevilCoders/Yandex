{
    "meta": {
        "name": "mdb-selfdns-client-plugins-mdb-porto",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "selfdns-client plugin for porto containers environment in MDB.",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
        "bin": {
            "targets": [
                "cloud/mdb/mdb-selfdns-client-plugins/cmd/mdb-porto"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-selfdns-client-plugins/cmd/mdb-porto/mdb-porto"
            },
            "destination": {
                "path": "/etc/yandex/selfdns-client/plugins/mdb-porto"
            }
        }
    ]
}
