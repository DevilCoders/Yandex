{% for cluster in salt['pillar.get']('data:walg-server:clusters', []) %}
/var/log/walg/{{ cluster }}.log {
    missingok
    rotate 10
    weekly
    compress
    notifempty
    delaycompress
    create 640 walg walg
}

/var/log/walg/{{ cluster }}-postgresql.log {
    missingok
    rotate 10
    weekly
    compress
    notifempty
    delaycompress
    create 640 walg walg
}
{% endfor %}
