domain ip6 {
    table filter {
        chain INPUT {
            proto tcp
            mod u32 u32 "0x10&0xffffffff={{ salt.pillar.get('data:zk:worker_project_id', '0xf802') }}"
            interface eth1 mod tcp
            saddr 2a02:6b8:c00::/40
            dport (2181 2281) ACCEPT;
        }
    }
}
