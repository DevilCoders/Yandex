{
    "meta": {
        "name": "dataproc-manager",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Service for managing datarpoc cluster health and jobs",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/dataproc-manager/cmd/dataproc-manager"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/dataproc-manager/cmd/dataproc-manager/dataproc-manager"
            },
            "destination": {
                "path": "/opt/yandex/dataproc-manager/bin/dataproc-manager"
            }
        }
    ]
}
