<yandex>
    <path>./</path>
    <background_move_processing_pool_thread_sleep_seconds>30</background_move_processing_pool_thread_sleep_seconds>
    <background_move_processing_pool_task_sleep_seconds_when_no_work_max>30</background_move_processing_pool_task_sleep_seconds_when_no_work_max>
    <mark_cache_size>5368709120</mark_cache_size>
    <storage_configuration>
        <disks>
            <default />
            <minio_s3>
                <type>s3</type>
                <endpoint>http://localhost:9000/root/data/</endpoint>
                <access_key_id>minio</access_key_id>
                <secret_access_key>minio123</secret_access_key>
            </minio_s3>
        </disks>
        <policies>
            <minio_s3>
                <volumes>
                    <main>
                        <disk>minio_s3</disk>
                    </main>
                </volumes>
            </minio_s3>
            <minio_hybrid>
                <volumes>
                    <main>
                        <disk>default</disk>
                    </main>
                    <external>
                        <disk>minio_s3</disk>
                    </external>
                </volumes>
            </minio_hybrid>
        </policies>
    </storage_configuration>

    <merge_tree>
        <allow_remote_fs_zero_copy_replication>1</allow_remote_fs_zero_copy_replication>
    </merge_tree>

    <tcp_port>19000</tcp_port>

    <http_port>8123</http_port>

    <remote_servers>
        <test_cluster>
            <shard>
                <replica>
                    <host>localhost</host>
                    <port>19000</port>
                </replica>
            </shard>
        </test_cluster>
    </remote_servers>

    <zookeeper>
        <node>
            <host>localhost</host>
            <port>2181</port>
        </node>
    </zookeeper>

    <macros>
        <cluster>test_cluster</cluster>
        <shard>shard1</shard>
    </macros>

    <users>
        <default>
            <password/>

            <networks>
                <ip>::/0</ip>
            </networks>

            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>

    <profiles>
        <default>
            <s3_max_single_part_upload_size>1024</s3_max_single_part_upload_size>
        </default>
    </profiles>

    <quotas>
        <default/>
    </quotas>

    <remote_url_allow_hosts>
        <host_regexp>^.*$</host_regexp>
    </remote_url_allow_hosts>

</yandex>