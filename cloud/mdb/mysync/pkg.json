{
    "meta": {
        "name": "mdb-mysync",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MySQL high availability tool",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/mdb/internal/mysync/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mysync/cmd/mysync"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mysync/cmd/mysync/mysync"
            },
            "destination": {
                "path": "/usr/local/bin/mysync"
            }
        }
    ]
}
