{
    "meta": {
        "name": "iam-token-reissuer",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "IAM Token Reissuer",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
        "bin": {
            "targets": [
                "cloud/mdb/tools/iam_token_reissuer"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/tools/iam_token_reissuer/cmd/iam_token_reissuer/iam_token_reissuer"
            },
            "destination": {
                "path": "/usr/bin/iam_token_reissuer"
            }
        }
    ]
}
