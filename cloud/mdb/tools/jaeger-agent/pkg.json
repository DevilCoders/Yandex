{
    "meta": {
        "name": "jaeger-agent",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Jaeger agent",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "yt/jaeger/legacy/agent"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "yt/jaeger/legacy/agent/agent"
            },
            "destination": {
                "path": "/usr/local/bin/jaeger-agent"
            }
        }
    ]
}
