{
    "meta": {
        "name": "grpcurl",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "cURL for gRPC",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "vendor/github.com/fullstorydev/grpcurl/cmd/grpcurl"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "vendor/github.com/fullstorydev/grpcurl/cmd/grpcurl/grpcurl"
            },
            "destination": {
                "path": "/usr/local/bin/grpcurl"
            }
        }
    ]
}
