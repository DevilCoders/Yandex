{
    "meta": {
        "name": "mdb-health",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Service for storing and reporting service- and host-based health",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mdb-health/cmd/mdb-health"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-health/cmd/mdb-health/mdb-health"
            },
            "destination": {
                "path": "/opt/yandex/mdb-health/bin/mdb-health"
            }
        }
    ]
}
