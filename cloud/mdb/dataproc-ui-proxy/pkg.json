{
    "meta": {
        "name": "dataproc-ui-proxy",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Dataproc UI Proxy",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/internal/teams/DataProc/UI-Proxy/"
    },
    "build": {
        "bin": {
            "targets": [
                "cloud/mdb/dataproc-ui-proxy/cmd/dataproc-ui-proxy"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/dataproc-ui-proxy/cmd/dataproc-ui-proxy/dataproc-ui-proxy"
            },
            "destination": {
                "path": "/opt/yandex/dataproc-ui-proxy/dataproc-ui-proxy"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/dataproc-ui-proxy/etc/dataproc-ui-proxy.service"
            },
            "destination": {
                "path": "/lib/systemd/system/dataproc-ui-proxy.service"
            }
        }
    ]
}
