{
    "meta": {
        "name": "mdb-event-producer",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "Service that sends MDB events to CloudEvents",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mdb-event-producer/cmd/mdb-event-producer",
                "cloud/mdb/mdb-event-producer/cmd/mdb-event-queue-unsent"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-event-producer/cmd/mdb-event-producer/mdb-event-producer"
            },
            "destination": {
                "path": "/opt/yandex/mdb-event-producer/bin/mdb-event-producer"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-event-producer/cmd/mdb-event-queue-unsent/mdb-event-queue-unsent"
            },
            "destination": {
                "path": "/opt/yandex/mdb-event-producer/bin/mdb-event-queue-unsent"
            }
        }
    ]
}
