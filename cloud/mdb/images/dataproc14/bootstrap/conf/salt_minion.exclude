log_level_logfile: debug
file_client: local
file_roots:
    base:
        - /srv/salt
enable_gpu_grains: False
disable_modules:
    - boto_cloudwatch
    - dockerng
    - glusterfs
    - ifttt
    - ipmi
    - nacl
    - npm
    - ps
    - rest_package
    - rest_service
    - rh_service
    - ssh_package
    - ssh_service
    - victorops
    - vsphere
    - win_servermanager
    - win_status
    - x509
