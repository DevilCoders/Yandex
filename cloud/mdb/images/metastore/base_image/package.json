{
    "meta": {
        "name": "metastore-server-base",
        "maintainer": "MDB",
        "description": "Metastore Base Image",
        "version": "latest"
    },
    "build": {
        "targets": [
            "cloud/mdb/images/metastore/base_image"
        ],
        "target-platforms": [
            "linux"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}
