{
    "meta": {
        "name": "mdb-tank-guns",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Tank Guns",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
        "mdb-clickhouse-gun": {
            "targets": [
                "cloud/mdb/benchmarks/pandora/cmd/mdb-clickhouse-gun"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "mdb-clickhouse-gun",
                "path": "cloud/mdb/benchmarks/pandora/cmd/mdb-clickhouse-gun/mdb-clickhouse-gun"
            },
            "destination": {
                "path": "/usr/local/bin/mdb-clickhouse-gun"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/mdb/benchmarks/pandora/configs",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/etc/yandex/mdb-tank/"
            }
        }
    ]
}
