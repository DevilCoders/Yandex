{
    "meta": {
        "name": "dbaas-cleaner",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "MDB Worker",
        "version": "1.{revision}",
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/cloud/mdb/cleaner"
    },
    "build": {
         "dbaas-cleaner": {
            "targets": [
                "cloud/mdb/cleaner/bin"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "dbaas-cleaner",
                "path": "cloud/mdb/cleaner/bin/dbaas-cleaner"
            },
            "destination": {
                "path": "/opt/yandex/dbaas-cleaner/bin/dbaas-cleaner"
            }
        }
    ]
}
