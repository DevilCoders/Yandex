{
    "meta": {
        "name": "mdb-secrets",
        "maintainer": "MDB <mdb@yandex-team.ru>",
        "description": "API for generating and accessing crypto keys",
        "version": "1.{revision}",
        "homepage": "https://wiki.yandex-team.ru/MDB/"
    },
    "build": {
         "bin": {
            "targets": [
                "cloud/mdb/mdb-secrets/cmd/mdb-secrets",
                "cloud/mdb/mdb-secrets/cmd/mdb-secrets-import"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-secrets/cmd/mdb-secrets/mdb-secrets"
            },
            "destination": {
                "path": "/opt/yandex/mdb-secrets/bin/mdb-secrets"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "bin",
                "path": "cloud/mdb/mdb-secrets/cmd/mdb-secrets-import/mdb-secrets-import"
            },
            "destination": {
                "path": "/opt/yandex/mdb-secrets/bin/mdb-secrets-import"
            }
        }
    ]
}
