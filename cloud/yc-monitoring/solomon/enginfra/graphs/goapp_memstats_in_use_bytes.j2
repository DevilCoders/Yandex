{% extends "../../base/graph.j2" %}
{% block parameters %}
    {
        "name": "project",
        "value": "{{ project_id }}"
    },
    {
        "name": "service",
        "value": "xds-provider"
    },
    {
        "name": "cluster",
        "value": "*"
    },
    {
        "name": "host",
        "value": "*"
    }
{% endblock %}
{% block elements %}
	{{
		render_element(
			"go_memstats_stack_inuse_bytes",
			"group_lines('sum', {sensor='go_memstats_stack_inuse_bytes'})",
		)
	}},
	{{
		render_element(
			"go_memstats_heap_inuse_bytes",
			"group_lines('sum', {sensor='go_memstats_heap_inuse_bytes'})",
		)
	}},
	{{
		render_element(
			"go_memstats_alloc_bytes",
			"group_lines('sum', {sensor='go_memstats_alloc_bytes'})",
		)
	}}
{% endblock %}
