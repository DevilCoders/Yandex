{% extends "../../../base/graph.j2" %}
{% block parameters %}
    {
        "name": "project",
        "value": "{{ project_id }}"
    },
    {
        "name": "service",
        "value": "xds-provider"
    },
    {
        "name": "cluster",
        "value": "*"
    },
    {
        "name": "host",
        "value": "*"
    }
{% endblock %}
{% block elements %}
	{{
		render_element(
			"[[host]] requests",
			"{sensor='dns_requests_total'}",
            area="true"
		)
	}},
	{{
		render_element(
			"[[host]] requests",
			"{sensor='dns_requests_errors'}",
            area="true", color="red",
		)
	}}
{% endblock %}
