{% extends "../../../base/graph.j2" %}
{% block parameters %}
    {
        "name": "project",
        "value": "{{ project_id }}"
    },
    {
        "name": "service",
        "value": "xds-provider"
    },
    {
        "name": "cluster",
        "value": "*"
    },
    {
        "name": "host",
        "value": "*"
    }
{% endblock %}
{% block elements %}
	{{
		render_element(
			"[[host]] invalid",
			"{sensor='services_invalid_total'}",
		)
	}},
	{{
		render_element(
			"[[host]] stale",
			"{sensor='services_stale_total'}",
		)
	}},
	{{
		render_element(
			"[[host]] ok state",
			"{sensor='services_updated_total'} + {sensor='services_ok_total'}",
		)
	}}
{% endblock %}
