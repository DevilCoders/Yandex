{
    "id": "compute-sys-stats",
    "rows": [
        {
            "panels": [
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "CPU usage on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&path=%2FSystem%2F*&host=[[host]]&graph=auto&secondaryGraphMode=none&cpu=all&norm=true&checks=-%2FSystem%2FUpTime%3B%2FSystem%2FUpTimeRaw"
                },
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "Load average on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&l.path=%2FProc%2FLa&l.host=[[host]]&graph=auto"
                },
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "Memory usage on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&l.path=%2FMemory%2FMemFree|%2FMemory%2FMemTotal|%2FMemory%2FMemAvailable&l.host=[[host]]&graph=auto&stack=false"
                }
            ]
        },
        {
            "panels": [
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "TxBytes on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&path=%2FNet%2FIfs%2FTxBytes&intf=*&host=[[host]]&graph=auto&stack=false"
                },
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "RxBytes on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&path=%2FNet%2FIfs%2FRxBytes&intf=*&host=[[host]]&graph=auto&stack=false"
                },
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "Filesystem used/free on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&host=[[host]]&graph=auto&secondaryGraphMode=none&l.path=%2FFilesystem%2FUsedB%7C%2FFilesystem%2FFreeB&norm=true"
                }
            ]
        },
        {
            "panels": [
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "Proc stats on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&path=%2FProc%2FProcs*&host=[[host]]&graph=auto&checks=-%2FProc%2FProcsSinceBoot&stack=false"
                },
                {
                    "subtitle": "",
                    "colspan": 0,
                    "title": "Disk utilization on [[host]]",
                    "markdown": "",
                    "type": "IFRAME",
                    "rowspan": 0,
                    "url": "/?project={{ project_id }}&cluster=[[cluster]]&service=sys&path=%2FIo%2FDisks%2FReadBytes|%2FIo%2FDisks%2FWriteBytes&host=[[host]]&graph=auto&stack=false"
                }
            ]
        }
    ],
    "heightMultiplier": 1.0,
    "name": "[sys] system metrics",
    "projectId": "{{ project_id }}",
    "description": "",
    "parameters": [
        {
            "value": "cloud_*",
            "name": "cluster"
        },
        {
            "value": "sys",
            "name": "service"
        },
        {
            "value": "*",
            "name": "host"
        }
    ]
}
