{% extends "../../base/graph.j2" %}

{% block parameters %}
{
	"name": "cluster",
	"value": "cloud_{{ env }}_ylb"
},
{
	"name": "service",
	"value": "loadbalancer_ctrl"
}
{% endblock %}

{% block elements %}
{{
	render_element(
		"get health error",
		"diff(group_lines('sum', {name='hc_ctrl_health_subscriber_get_health_fail'}))",
		color="red",
		area="true",
	)
}},
{{
	render_element(
		"get health success",
		"diff(group_lines('sum', {name='hc_ctrl_health_subscriber_get_health_success'}))",
		color="cyan",
		area="true",
	)
}}
{% endblock %}
