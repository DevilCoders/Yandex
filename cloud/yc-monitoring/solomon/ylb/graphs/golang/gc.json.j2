{% extends "base.json.j2" %}

{% block parameters %}
{
	"name": "cluster",
	"value": "cloud_{{ env }}_ylb"
}
{% endblock %}

{% block elements %}
{{
	render_element(
		"gc_pauses_ns",
		"non_negative_derivative(group_lines('sum', {name='gc_pauses_ns'}) / 1000000)",
	)
}},
{{
	render_element(
		"gcCycles",
		"non_negative_derivative(group_lines('sum', {name='gcCycles'}))",
		yaxis="RIGHT",
	)
}}
{% endblock %}
