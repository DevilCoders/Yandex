{% extends "base.json.j2" %}

{% block parameters %}
{
	"name": "cluster",
	"value": "cloud_{{ env }}_ylb"
}
{% endblock %}

{% block elements %}
{{
	render_element(
		"heap_obj_allocs",
		"non_negative_derivative(group_lines('sum', {name='heap_obj_allocs'}))",
	)
}},
{{
	render_element(
		"heap_obj_lives",
		"non_negative_derivative(group_lines('sum', {name='heap_obj_lives'}))",
	)
}}
{% endblock %}
