{% extends "base.j2" %}

{% set id = "{}-{}-{}".format(prefix, env, "grpc-latency") %}
{% set name = team + " gRPC latency (" + env + ")" %}
{% set transform = 'NONE' %}

{% block additional_params %}
    ,{
        "name": "host",
        "value": "*"
    }
    ,{
        "name": "zone",
        "value": "{{ az_zones_for_parameter }}"
    }
    ,{
        "name": "app",
        "value": "*"
    }
    ,{
        "name": "method",
        "value": "*"
    }
{% endblock %}

{% block elements %}
    {{ grpc_all_percentiles(app='none') }}
{% endblock %}
