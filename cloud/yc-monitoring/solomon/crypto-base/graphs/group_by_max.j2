{% extends "../../base/graph.j2" %}

{% block parameters %}
    {
        "name": "project",
        "value": "{{ project_id }}"
    },
    {
      "name": "host",
      "value": "*"
    }
{% endblock %}

{% block elements %}
	{{
	    render_element(
			title,
			expression="group_by_time(15s, 'max', {cluster='%s', %s='%s', service='%s', host='[[host]]'})" | format(cluster, sensor_label, sensor, solomon_service)
		)
	}}
{% endblock %}
