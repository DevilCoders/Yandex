{
    "version": 1,
    "checks": [
      {
        "check_script": "passive-check-deliver.py",
        "services": ["passive-check-deliver"],
        "run_always": true,
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "freespace.py",
        "services": ["freespace"],
        "interval": 300,
        "timeout": 30
      },
      {
        "check_script": "coredumps.py",
        "services": ["coredumps"],
        "interval": 60,
        "timeout": 30
      },
      {
        "check_script": "oom-killer.sh",
        "services": ["oom-killer"],
        "interval": 60,
        "timeout": 30
      },
      {
        "check_script": "reboot-count.py",
        "services": ["reboot-count"],
        "interval": 300,
        "timeout": 30
      },
      {
        "check_script": "solomon-agent.py",
        "services": ["solomon-agent"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "push-client.sh",
        "services": ["push-client"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "platform-http-check.py",
        "args": ["--service", "envoy-front"],
        "services": ["envoy-front"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "k8s-master-component.py",
        "args": ["--component", "yandex-ccm", "--service", "k8s-master-yandex-ccm"],
        "services": ["k8s-master-yandex-ccm"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "k8s-master-component.py",
        "args": ["--component", "authnwebhook", "--service", "k8s-master-authnwebhook"],
        "services": ["k8s-master-authnwebhook"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "k8s-master-component.py",
        "args": ["--component", "csi-provisioner", "--service", "k8s-master-csi-provisioner"],
        "services": ["k8s-master-csi-provisioner"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "k8s-master-component.py",
        "args": ["--component", "csi-liveness-probe", "--service", "k8s-master-csi-liveness-probe"],
        "services": ["k8s-master-csi-liveness-probe"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "k8s-master-component.py",
        "args": ["--component", "csi-attacher", "--service", "k8s-master-csi-attacher"],
        "services": ["k8s-master-csi-attacher"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "k8s-master-component.py",
        "args": ["--component", "csi-controller", "--service", "k8s-master-csi-controller"],
        "services": ["k8s-master-csi-controller"],
        "interval": 15,
        "timeout": 30
      },
      {
        "check_script": "platform-http-check.py",
        "args": ["--service", "fluentd-metrics"],
        "services": ["fluentd-metrics"],
        "interval": 15,
        "timeout": 30
      }
    ],
    "env": {
        "USER": "root"
    }
}
