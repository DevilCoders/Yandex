#!/usr/bin/make -f
# -*- makefile -*-

build:
	cd python_client; \
	${PYTHON} setup.py easy_install twine wheel; \
	${PYTHON} setup.py clean --all; \
	${PYTHON} setup.py "${SETUPTOOLS_COMMAND}"

	cd python_client/dist; \
	twine upload --repository-url "${REPOSITORY_URL}" ./*

clean:
	echo "no cleanup needed"

binary:
	echo "no binary needed"
