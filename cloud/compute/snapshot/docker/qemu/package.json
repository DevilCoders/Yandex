{
  "meta": {
    "name": "cloud-snapshot-qemu-docker",
    "maintainer": "compute <compute@yandex-team.ru>",
    "description": "Docker image for isolation qemu while if convert customer images",
    "version": "rev-{revision}.br-{branch}.{sandbox_task_id}"
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "cloud/compute/snapshot/docker/qemu/entrypoint.sh"
      },
      "destination": {
        "path": "/entrypoint.sh"
      }
    }
  ]
}