{
  "meta": {
    "name": "yc-compute-golang",
    "maintainer": "Cloud compute <cloud-compute@yandex-team.ru>",
    "description": "Golang package from official tarball",
    "depends": ["debhelper (>= 8.0.0)"],
    "version": "1.16.5-sandbox-task-{sandbox_task_id}.arc-revision-{revision}",
    "noconffiles_all": false
  },
  "data": [
    {
      "source":{
        "type": "ARCADIA",
        "path": "cloud/compute/golang-package/compute-golang.sh"
      },
      "destination": {
        "path":"/etc/profile.d/compute-golang.sh"
      }
    },
    {
      "source": {
        "type": "SANDBOX_RESOURCE",
        "id": 2298436542,
        "untar": true
      },
      "destination": {
        "path": "/usr/local/go-compute/"
      }
    }
  ]
}
