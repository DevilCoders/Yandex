{
    "meta": {
        "name": "dist",
        "maintainer": "yc-billing",
        "version": "dummy",
    },
    "build": {
        "build_type": "release",
        "targets": [
            "contrib/python/awscli/bin",
            "cloud/billing/bootstrap/internal/packaging/common"
        ],
        "target-platforms": [
            "DEFAULT-LINUX-X86_64"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "contrib/python/awscli/bin/aws"
            },
            "destination": {
                "path": "/"
            }
        },
        {
           "source": {
                "type": "RELATIVE",
                "path": "scripts/dist.sh"
            },
            "destination": {
                "path": "/"
            }
        },
        {
           "source": {
                "type": "RELATIVE",
                "path": "scripts/download_preprod.sh"
            },
            "destination": {
                "path": "/preprod/download.sh"
            }
        },
        {
           "source": {
                "type": "RELATIVE",
                "path": "scripts/download_prod.sh"
            },
            "destination": {
                "path": "/prod/download.sh"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/billing/bootstrap/internal/packaging/common/common"
            },
            "destination": {
                "path": "/preprod/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/billing/bootstrap/internal/packaging/common/common"
            },
            "destination": {
                "path": "/prod/"
            }
        }
    ]
}
