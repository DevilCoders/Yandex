# SSH keys Specific
# Use this if oslogin fails
# NOTE: configured yav client needed

locals {
  abc_group = "yandexcloudbilling"
}

module "billing-abc-ssh" {
  source       = "../modules/ssh-keys"
  yandex_token = module.yandex_token.token
  abc_service  = local.abc_group
}

output "ssh_users" {
  value = module.billing-abc-ssh.logins
}
