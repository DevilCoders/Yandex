{
    "meta": {
        "name": "yc-billing-terraform",
        "maintainer": "yc-billing",
        "description": "Package with terraform binaries for CI. Build from {revision}@{branch}",
        "version": "0.2"
    },
    "build": {
        "build_type": "release",
        "targets": [
            "cloud/billing/go/deploy/ci_tools/utils/juggler_cli",
            "cloud/billing/go/deploy/ci_tools/utils/yt-oauth"
        ],
        "target-platforms": [
            "DEFAULT-LINUX-X86_64"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/billing/go/deploy/ci_tools/utils/juggler_cli"
            },
            "destination": {
                "path": "/juggler_cli"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/billing/go/deploy/ci_tools/utils/yt-oauth"
            },
            "destination": {
                "path": "/yt-oauth"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "files"
            },
            "destination": {
                "path": "/files"
            }
        }
    ]
}
