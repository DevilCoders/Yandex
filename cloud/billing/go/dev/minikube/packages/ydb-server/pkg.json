{
  "meta": {
    "name": "dev-ydb",
    "maintainer": "drew <drew@yandex-team.ru>",
    "description": "Package with local ydb and logbroker servers over stable kikimr",
    "version": "stable"
  },
  "build": {
    "targets": [
      "kikimr/public/tools/package/stable",
      "kikimr/public/tools/lbk_recipe",
      "kikimr/public/tools/ydb"
    ],
    "target-platforms": [
      "DEFAULT-LINUX-X86_64"
    ]
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "kikimr/public/tools/package/stable/Berkanavt/kikimr/bin/kikimr"
      },
      "destination": {
        "path": "/kikimr"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "kikimr/public/tools/ydb/ydb"
      },
      "destination": {
        "path": "/ydb",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "kikimr/public/tools/lbk_recipe/lbk_recipe"
      },
      "destination": {
        "path": "/lbk_recipe",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    }
  ]
}
