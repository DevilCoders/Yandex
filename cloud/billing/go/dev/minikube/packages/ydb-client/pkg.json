{
  "meta": {
    "name": "dev-ydb-client",
    "maintainer": "drew <drew@yandex-team.ru>",
    "description": "Package with ydb client",
    "version": "stable"
  },
  "build": {
    "targets": [
      "kikimr/public/tools/ydb",
      "logbroker/public/client/bin"
    ],
    "target-platforms": [
      "DEFAULT-LINUX-X86_64"
    ]
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "files"
      },
      "destination": {
        "path": "/files"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "kikimr/public/tools/ydb/ydb"
      },
      "destination": {
        "path": "/ydb",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "logbroker/public/client/bin/logbroker"
      },
      "destination": {
        "path": "/logbroker",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    }
  ]
}
