{
"meta": {
    "name": "mdb-support-bot",
        "maintainer": "Danil Chernyuk <hexyo@yandex-team.ru>",
        "description": "MDB-Support personal bot with schedule",
        "version": "1.1.2"
    },
    "build": {
            "targets": [
              "cloud/support/mdb-support-bot"
            ],
            "target-platforms": [
              "linux"
            ],
    "targets": [
      "cloud/support/mdb-support-bot"
    ],
    "target-platforms": [
      "linux"
    ]
  },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/support/mdb-support-bot"
            },
            "destination": {
                "path": "/mdb-support-bot"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "root.crt"
            },
            "destination": {
                "path": "/root.crt"
            }
        },
    ]
}
