{
    "meta": {
        "name": "yandex-netmon-agent",
        "version": "0.9.1.8-{revision}",
        "description": "Netmon agent binary for cloud",
        "maintainer": "Anatoly Tsyganenko <atsygane@yandex-team.ru>"
    },
    "build": {
        "targets": [
            "juggler/netmon/agent"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "juggler/netmon/agent/netmon-agent"
            },
            "destination": {
                "path": "/usr/local/bin/"
            }
        }
    ]
}
