EXECTEST()
OWNER(g:cloud-dwh)

DEPENDS(
    infra/dsl_jsonnet/tools/jsonnet
    contrib/python/juggler_sdk/cli
)

DATA(
    arcadia/cloud/dwh/monitoring
)

REQUIREMENTS(
    network:full
)

RUN(
    NAME generate_json
    jsonnet ${ARCADIA_ROOT}/cloud/dwh/monitoring/init.jsonnet
    STDOUT output.json
)

RUN(
    NAME try_juggler_cli
    STDIN output.json
    juggler_cli load -c
)

END()
