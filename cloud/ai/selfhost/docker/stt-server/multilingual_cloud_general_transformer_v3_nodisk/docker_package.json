{
  "meta": {
    "version": "stt-server-{revision}-v3-ds-multilang",
    "maintainer": "Ermilov Anton <eranik@yandex-team.ru>",
    "name": "stt-server",
    "description": "ASR server"
  },
  "build": {
    "targets" : [
      "voicetech/asr/cloud_engine/server/stt/app",
      "voicetech/asr/lingware/multilingual_cloud_general_transformer_streaming"
    ],
    "build_type": "release",
    "flags": [
      {
	    "name": "CUDA_VERSION",
	    "value": "11.4"
      },
      {
        "name": "TENSORRT_VERSION",
        "value": "7"
      },
      {
        "name": "CUDNN_VERSION",
        "value": "8.0.5"
      },
      {
        "name": "OS_SDK",
        "value": "ubuntu-16"
      }
    ]
  },
  "data": [
    {
      "source": {
        "path": "voicetech/asr/cloud_engine/server/stt/app/asr_server",
        "type": "BUILD_OUTPUT"
      },
      "destination": {
        "path": "/asr_server"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "run.sh"
      },
      "destination": {
        "path": "/run.sh"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "path": "voicetech/asr/lingware/multilingual_cloud_general_transformer_streaming",
        "type": "BUILD_OUTPUT"
      },
      "destination": {
        "path": "/lingware"
      }
    }
  ]
}
