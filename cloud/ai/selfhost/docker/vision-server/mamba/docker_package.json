{
    "meta": {
        "version": "mamba-{revision}",
        "maintainer": "Gleb Yengalych <herr-bilbo@yandex-team.ru>",
        "name": "vision-server",
        "description": "vision server"
    },
    "build": {
        "targets": [
            "voicetech/asr/cloud_engine/server/vision",
            "extsearch/images/daemons/cbirdaemon2"
        ]
    },
    "data": [
        {
            "source": {
                "path": "voicetech/asr/cloud_engine/server/vision",
                "type": "BUILD_OUTPUT"
            },
            "destination": {
                "path": "/vision"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "extsearch/images/daemons/cbirdaemon2"
            },
            "destination": {
                "path": "/cbirdaemon2"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "launch.sh"
            },
            "destination": {
                "path": "/launch.sh"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "configs/graph.pb"
            },
            "destination": {
                "path": "/vision/launch_cbirdaemon2/data/graph.pb"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "configs/cbir_data.conf"
            },
            "destination": {
                "path": "/vision/launch_cbirdaemon2/data/cbir_data.conf"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "configs/cbir_prod_config.conf"
            },
            "destination": {
                "path": "/vision/launch_cbirdaemon2/prod_configs/cbir_prod_config.conf"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "configs/config.json"
            },
            "destination": {
                "path": "/vision/python/server/custom_config.json"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        }
    ]
}

