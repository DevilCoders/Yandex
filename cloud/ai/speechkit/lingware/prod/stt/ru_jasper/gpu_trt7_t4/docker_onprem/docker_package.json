{
  "meta": {
    "version": "jasper-t4-{revision}",
    "maintainer": "Ermilov Anton <eranik@yandex-team.ru>",
    "name": "stt-server-onprem",
    "description": "ASR server"
  },
  "build": {
    "targets" : [
      "voicetech/asr/cloud_engine/server/stt/app_onprem",
      "cloud/ai/speechkit/lingware/prod/stt/ru_jasper/gpu_trt7_t4/resources"
    ],
    "build_type": "release",
    "flags": [
      {
	    "name": "CUDA_VERSION",
	    "value": "11.4"
      },
      {
        "name": "TENSORRT_VERSION",
        "value": "7"
      },
      {
        "name": "CUDNN_VERSION",
        "value": "8.0.5"
      },
      {
        "name": "OS_SDK",
        "value": "ubuntu-16"
      }
    ]
  },
  "data": [
    {
      "source": {
        "path": "voicetech/asr/cloud_engine/server/stt/app_onprem/asr_server",
        "type": "BUILD_OUTPUT"
      },
      "destination": {
        "path": "/asr_server"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "path": "cloud/ai/speechkit/lingware/prod/stt/ru_jasper/gpu_trt7_t4/resources",
        "type": "BUILD_OUTPUT"
      },
      "destination": {
        "path": "/lingware"
      }
    },
    {
      "source": {
        "path": "cloud/ai/speechkit/lingware/prod/stt/ru_jasper/gpu_trt7_t4/resources/fst_normalizer.tar.gz",
        "type": "BUILD_OUTPUT",
        "untar": true
      },
      "destination": {
        "path": "/lingware/"
      }
    }
  ]
}
