{
  "Root": "${conf}",
  "Lang": "ru",
  "Abbr": "nn_normalizer/ru.abbr.txt",
  "TLDList": "nn_normalizer/tld-list.txt",
  "AllowedWords": "nn_normalizer/allowed_words.txt",
  "NumWorkers": 1,
  "ModeParams": {
    "Mode": "local"
  },
  "Tokenizer": {
    "Params": "3;keep-io;keep-renyxa",
    "Filters": [
      "TokenNormalizerFilter",
      "SegmenterFilter;mode=BPE;file=nn_normalizer/src_bpe.voc;sentinels=1;langSplit=0;maxTokens=512"
    ]
  },
  "SrcVoc": {
    "Path": "nn_normalizer/src.voc",
    "BosId": 0,
    "EosId": 1,
    "UnkId": 2
  },
  "DstVoc": {
    "Path": "nn_normalizer/dst.voc",
    "BosId": 0,
    "EosId": 1,
    "UnkId": 2
  },
  "Model": {
    "ModelPath": "nn_normalizer/transformer.npz",
    "Devices": [
      -1
    ],
    "NumSessionsPerDevice": 1,
    "NumInterOpThreads": 1,
    "NumIntraOpThreads": 1,
    "NumThreadsPerSession": 1,
    "NumPeers": 0,
    "BatchSize": 1,
    "BeamSize": 4,
    "BeamSpread": 3.0,
    "LenAlpha": 1.0,
    "AttnBeta": 0.0
  },
  "NeuralTranslator": {
    "MaxLen": 512,
    "MaxUnkCount": 5,
    "MaxUnkFraction": 0.4,
    "MaxRepeats": 10,
    "EmptyOnFailure": true
  },
  "TextNormalizer": {
    "SwearSymbol": "â€¼"
  }
}
