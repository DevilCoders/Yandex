{
    "meta": {
        "name": "speechkit",
        "maintainer": "g:cloud-asr",
        "description": "SpeechKit ",
        "version": "0.4"
    },
    "build": {
        "targets": [
            "cloud/ai/public/speechkit",
            "cloud/bitbucket/public-api/yandex/cloud/ai/tts/v3",
            "cloud/bitbucket/public-api/yandex/cloud/ai/stt/v3",
            "cloud/bitbucket/public-api/yandex/cloud/ai/stt/v2",
            "cloud/bitbucket/public-api/yandex/cloud/operation",
            "cloud/bitbucket/common-api/yandex/cloud/api",
            "cloud/bitbucket/common-api/yandex/cloud/api/tools",
            "contrib/libs/googleapis-common-protos"
        ],
        "add-result": [
            ".py"
        ]
    },
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/public/speechkit",
                "files": [
                    "*.py"
                ]
            },
            "destination": {
                "path": "/speechkit/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/bitbucket/public-api",
                "files": [
                    "*.py"
                ]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/bitbucket/common-api",
                "files": [
                    "*.py"
                ]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "contrib/libs/googleapis-common-protos",
                "files": [
                    "*.py"
                ]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/public/python-package/setup.py"
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
