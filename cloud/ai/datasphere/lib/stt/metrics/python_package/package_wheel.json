{
    "meta": {
        "name": "stt-metrics",
        "maintainer": "eranik <eranik@yandex-team.ru>",
        "description": "STT metrics package",
        "version": "0.2"
    },
    "build": {
        "targets": [
            "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/alignment_utils.so"
            },
            "destination": {
                "path": "/stt_metrics/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/__init__.py"
            },
            "destination": {
                "path": "/stt_metrics/__init__.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/common.py"
            },
            "destination": {
                "path": "/stt_metrics/common.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/resources.py"
            },
            "destination": {
                "path": "/stt_metrics/resources.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/version.py"
            },
            "destination": {
                "path": "/stt_metrics/version.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/wer.py"
            },
            "destination": {
                "path": "/stt_metrics/wer.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/text_transform.py"
            },
            "destination": {
                "path": "/stt_metrics/text_transform.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/normalizer_service/normalizer_pb2.py"
            },
            "destination": {
                "path": "/stt_metrics/normalizer_service/normalizer_pb2.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/stt_metrics/normalizer_service/normalizer_pb2_grpc.py"
            },
            "destination": {
                "path": "/stt_metrics/normalizer_service/normalizer_pb2_grpc.py"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "cloud/ai/datasphere/lib/stt/metrics/python_package/setup.py"
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
