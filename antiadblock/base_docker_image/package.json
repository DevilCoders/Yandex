{
  "meta": {
    "name": "baseimage",
    "maintainer": "Antiadblock team",
    "version": "{revision}",
    "description": "Base Docker image for Antiadblock cryprox",
    "homepage": "https://wiki.yandex-team.ru/ANTIADB/"
  },
  "build": {
    "release": {
      "targets": [
        "antiadblock/base_docker_image"
      ],
      "build_type": "release"
    }
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/base_docker_image/apt-sources"
      },
      "destination": {
        "path": "/etc/apt/sources.list.d"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/base_docker_image/Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "SANDBOX_RESOURCE",
        "id": 1097977922,
        "untar": false,
        "path": "logrotate"
      },
      "destination": {
        "path": "/usr/bin/logrotate",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    }
  ]
}
