{
  "meta": {
    "name": "cryprox-package-for-sandbox",
    "maintainer": "Antiadblock team",
    "version": "master",
    "description": "Crypting-proxy antiadblock service (cryprox+nginx)",
    "homepage": "https://wiki.yandex-team.ru/ANTIADB/"
  },
  "build": {
    "debug": {
      "targets": [
        "antiadblock/cryprox",
        "antiadblock/cryprox/cryprox_run",
        "antiadblock/cryprox/nginx"
      ],
      "flags": [
        {
          "name": " NO_STRIP",
          "value": "yes"
        }
      ],
      "build_type": "debug"
    },
    "release": {
      "targets": [
        "antiadblock/cryprox",
        "antiadblock/cryprox/cryprox_run",
        "antiadblock/cryprox/nginx"
      ],
      "build_type": "release"
    }
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "build_key": "release",
        "path": "antiadblock/cryprox/cryprox_run"
      },
      "destination": {
        "path": "/bin/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/nginx"
      },
      "destination": {
        "path": "/etc/nginx"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/sandbox/init_instance_for_sandbox.sh"
      },
      "destination": {
        "path": "/usr/bin/init_instance_for_sandbox.sh",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/sandbox/make_nginx_conf.sh"
      },
      "destination": {
        "path": "/usr/bin/make_nginx_conf.sh",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/update_resources.py"
      },
      "destination": {
        "path": "/usr/bin/update_resources.py",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    }
  ]
}
