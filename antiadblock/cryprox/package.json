{
  "meta": {
    "name": "uni",
    "maintainer": "Antiadblock team",
    "version": "{revision}",
    "description": "Crypting-proxy antiadblock service (cryprox+nginx)",
    "homepage": "https://wiki.yandex-team.ru/ANTIADB/"
  },
  "build": {
    "debug": {
      "targets": [
        "antiadblock/cryprox",
        "antiadblock/cryprox/cryprox_run",
        "antiadblock/cryprox/nginx",
        "antiadblock/tools/deploy_unit_resolver",
        "antiadblock/cryprox/log_shippers/filebeat/update_filebeat"
      ],
      "flags": [
        {
          "name": " NO_STRIP",
          "value": "yes"
        }
      ],
      "build_type": "debug"
    },
    "release": {
      "targets": [
        "antiadblock/cryprox",
        "antiadblock/cryprox/cryprox_run",
        "antiadblock/cryprox/nginx",
        "antiadblock/tools/deploy_unit_resolver",
        "antiadblock/cryprox/log_shippers/filebeat/update_filebeat"
      ],
      "build_type": "release"
    }
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "build_key": "release",
        "path": "antiadblock/cryprox/cryprox_run"
      },
      "destination": {
        "path": "/bin/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/nginx"
      },
      "destination": {
        "path": "/etc/nginx"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "build_key": "release",
        "path": "antiadblock/tools/deploy_unit_resolver"
      },
      "destination": {
        "path": "/usr/bin"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/logrotate.conf"
      },
      "destination": {
        "path": "/etc/logrotate/logrotate.conf",
        "attributes": {
          "mode": {
            "value": "a-w"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/log_shippers"
      },
      "destination": {
        "path": "/log_shippers"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "build_key": "release",
        "path": "antiadblock/cryprox/log_shippers/filebeat/update_filebeat"
      },
      "destination": {
        "path": "/log_shippers/update_filebeat"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/init_instance_container.sh"
      },
      "destination": {
        "path": "/usr/bin/init_instance_container.sh",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/update_resources.py"
      },
      "destination": {
        "path": "/usr/bin/update_resources.py",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/unbound/unbound.conf"
      },
      "destination": {
        "path": "/etc/unbound/unbound.conf"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/unbound/resolv.conf"
      },
      "destination": {
        "path": "/etc/resolv.conf"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/monitoring/solomon-agent"
      },
      "destination": {
        "path": "/solomon-agent"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/cryprox/monitoring/juggler/checks"
      },
      "destination": {
        "path": "/juggler/checks"
      }
    }
  ]
}
