{
    "template": "staging-antiadb-cryprox-filebeat-*",
    "settings": {
        "index": {
            "refresh_interval": "180s",
            "number_of_shards": 1,
            "number_of_replicas": 1,
            "translog": {
                "durability": "async"
            },
            "unassigned": {
                "node_left": {
                    "delayed_timeout": "30m"
                }
            }
        }
    },
    "aliases" : {
        "test-antiadb-cryprox-filebeat" : {}
    },
    "mappings": {
        "dynamic_templates": [{
            "strings": {
                "match_mapping_type": "string",
                "mapping": {
                    "type": "keyword"
                }
            }
        }],
        "properties": {
            "@timestamp": { "type": "date" },
            "host": { "type": "keyword" },
            "tags": { "type": "keyword" },
            "request_tags": { "type": "keyword" },
            "request_id": { "type": "keyword" },
            "service_id": { "type": "keyword" },
            "exception": { "type": "text", "norms": false },
            "human_readable_reason": { "type": "text", "norms": false },
            "meta": { "type": "keyword", "ignore_above": 8191 },
            "action": { "type": "keyword" },
            "duration": { "type": "integer" },
            "url": { "type": "text", "norms": false },
            "http_host": { "type": "keyword" },
            "http_code": { "type": "short" },
            "content_type": { "type": "keyword" },
            "content_length": { "type": "integer" },
            "cfg_version": { "type": "keyword"},
            "seed": { "type": "keyword"},
            "status": { "type": "keyword"}
        }
    }
}
