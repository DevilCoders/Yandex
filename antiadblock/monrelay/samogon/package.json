{
  "meta": {
    "name": "aab-monrelay",
    "maintainer": "antiadblock group",
    "version": "{branch}@{revision}"
  },
  "build": {
    "targets": [
      "antiadblock/monrelay/samogon"
    ]
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/monrelay/samogon/plugin"
      },
      "destination": {
        "path": "/plugin"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "antiadblock/monrelay"
      },
      "destination": {
        "path": "/plugin/monrelay"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "antiadblock/monrelay/samogon/monrelay"
      },
      "destination": {
        "archive": "/linux/monrelay.tar.gz"
      }
    }
  ]
}
