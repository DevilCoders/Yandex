{
  "meta": {
    "name": "elastic",
    "maintainer": "Antiadblock team",
    "version": "{revision}",
    "description": "antiadblock elasticsearch cluster"
  },
  "build": {
    "release": {
      "targets": [
        "antiadblock/elastic/elasticsearch",
        "antiadblock/tools/deploy_unit_resolver"
      ],
      "build_type": "release"
    }
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/elastic/elasticsearch/config"
      },
      "destination": {
        "path": "/usr/share/elasticsearch/config"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/elastic/elasticsearch/solomon-agent"
      },
      "destination": {
        "path": "/solomon-agent"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/elastic/elasticsearch/Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/elastic/elasticsearch/launch"
      },
      "destination": {
        "path": "/launch"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "build_key": "release",
        "path": "antiadblock/tools/deploy_unit_resolver"
      },
      "destination": {
        "path": "/usr/bin"
      }
    },
    {
      "source": {
        "type": "SANDBOX_RESOURCE",
        "id": 1330601782,
        "untar": false
      },
      "destination": {
        "path": "/usr/bin/solomon-agent",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    }
  ]
}
