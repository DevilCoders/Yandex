{
  "meta": {
    "name": "aab-configs_api",
    "maintainer": "antiadblock group",
    "version": "master"
  },
  "build": {
    "targets": [
      "antiadblock/configs_api/samogon",
      "tools/pgmigrate"
    ]
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/configs_api/samogon/plugin"
      },
      "destination": {
        "path": "/plugin"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "antiadblock/configs_api/migrations",
        "files": [
          "*"
        ]
      },
      "destination": {
        "archive": "/linux/migrations.tar.gz"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "antiadblock/configs_api"
      },
      "destination": {
        "archive": "/linux/configs_api.tar.gz"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "tools/pgmigrate"
      },
      "destination": {
        "archive": "/linux/pgmigrate.tar.gz"
      }
    }
  ]
}
