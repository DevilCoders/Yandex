{
  "meta": {
    "name": "antiadblock-configs-api",
    "maintainer": "antiadblock team",
    "version": "{revision}",
    "description": "configs source for cryprox service",
    "homepage": "https://wiki.yandex-team.ru/ANTIADB/"
  },
  "build": {
    "debug": {
      "targets": [
        "antiadblock/configs_api/bin"
      ],
      "flags": [
        {
          "name": " NO_STRIP",
          "value": "yes"
        }
      ],
      "build_type": "debug"
    },
    "release": {
      "targets": [
        "antiadblock/configs_api/bin"

      ],
      "build_type": "release"
    }
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "build_key": "release",
        "path": "antiadblock/configs_api/bin"
      },
      "destination": {
        "path": "/bin/"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/"
      }
    }
  ]
}
