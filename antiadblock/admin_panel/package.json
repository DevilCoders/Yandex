    "null-loader": "0.1.1",
    "postcss": "7.0.14",
    "postcss-cssnext": "2.11.0",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.8.6",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.3.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-no-browser-hacks": "1.1.0",
    "stylelint-suitcss": "1.0.0",
    "svg-url-loader": "2.3.0",
    "svgo-loader": "1.2.1",
    "url-loader": "0.6.2",
    "webpack": "3.12.0",
    "webpack-node-externals": "1.6.0"
  }
}
