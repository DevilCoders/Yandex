{
  "name": "AntiAdB",
  "version": "0.0.1",
  "description": "AntiAdBlock",
  "main": "index.js",
  "private": true,
  "scripts": {
    "magic": "magicdev --color | bunyan -o short",
    "local-prod": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=local-prod node ./ --color | bunyan -o short",
    "build": "NODE_ENV=production webpack --hide-modules --config ./webpack/webpack.config.js",
    "i18n": "tanker pull",
    "test": "npm run lint && npm run jest",
    "lint": "eslint --ext .js,.jsx ./ && stylelint app/**/*.css",
    "js-lint-fix": "eslint --ext .js,.jsx ./ --fix",
    "jest": "NODE_ENV=test BEM_LANG=ru jest --silent --no-cache",
    "jest-coverage": "NODE_ENV=test BEM_LANG=ru jest --silent --coverage",
    "jest-coverage-not-silent": "NODE_ENV=test BEM_LANG=ru jest --coverage",
    "update-local-deps": "npm i && npm run local-deps",
    "local-deps": "npm install --no-save @yandex-int/magicdev@2.8.7 tanker-kit@1.6.8 webpack-dev-middleware@1.12.0 webpack-hot-middleware@2.20.0 git-hooks@^1.1.8",
    "tanker-install": "npm install --no-save tanker-kit@1.6.8",
    "git-hooks": "node_modules/git-hooks/bin/git-hooks --install",
    "check-valid-trendbox-config": "npx --no-install trendbox-cli check"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.js",
      "app/**/*.jsx",
      "!app/i18n/**"
    ],
    "automock": false,
    "setupFiles": [
      "./test/setup.js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^app(.*)$": "<rootDir>/app$1"
    }
  },
  "author": "artyom-void <artyom-void@yandex-team.ru>",
  "license": "MIT",
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "csp-preset-yastatic": "0.1.0",
    "express": "4.16.2",
    "express-bunyan-logger": "1.3.2",
    "express-http-geobase": "4.0.0",
    "express-http-langdetect": "4.0.0",
    "express-tld": "2.1.0",
    "express-uatraits-mock": "1.0.0",
    "express-x-forwarded-for-fix": "1.1.3",
    "express-ya-frameguard": "1.0.0",
    "express-yandex-csp": "3.1.0",
    "express-yandexuid": "6.0.0",
    "helmet": "3.16.0",
    "immutability-helper": "3.0.0",
    "inherit": "2.2.6",
    "js-priority-queue": "0.1.5",
    "js-yaml": "3.11.0",
    "lego-on-react": "git+https://github.yandex-team.ru/alex-kuz/lego-on-react.git",
    "lodash": "4.17.4",
    "prop-types": "15.6.0",
    "query-string": "5.1.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-highcharts": "16.0.2",
    "react-hot-loader": "3.1.3",
    "react-markdown": "4.0.3",
    "react-monaco-editor": "0.13.0",
    "react-redux": "6.0.1",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "source-map-support": "0.5.0",
    "vow-got": "1.1.0",
    "whatwg-fetch": "2.0.3",
    "winston": "2.4.0",
    "yandex-config": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-node6": "0.4.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "bunyan": "1.8.12",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "0.28.7",
    "dayjs": "1.8.15",
    "enzyme": "2.9.1",
    "enzyme-to-json": "2.0.0",
    "eslint": "5.16.0",
    "eslint-config-xo": "0.26.0",
    "eslint-config-xo-react": "0.19.0",
