CREATE TABLE events (
    event_id INTEGER PRIMARY KEY NOT NULL, -- automatically generated id
    event_type TEXT NOT NULL, -- event type: GROUP, TAG, INTLOOKUP, HOST, ...
    event_name TEXT NOT NULL, -- specific for every event type actions: ADDED HWMODIFIED GROUPCHANGED REMOVED for HOST, ...
    event_object_id TEXT NOT NULL, -- event identifier: for GROUP - group names, for HOST - fqdn host name, for TAG - tag name
    event_params TEXT NOT NULL, -- dict with extra event params
    event_date INTEGER NOT NULL -- date when event was added
);
