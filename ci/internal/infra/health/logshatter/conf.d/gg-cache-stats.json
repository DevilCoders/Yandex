{
    "logHosts": "*",
    "logPath": "**",
    "clickhouseTable": "gg.cache_stats",
    "dataRotationDays": 732,
    "sources": [
        "logbroker://autocheck_sandbox_tasks--stages"
    ],
    "jsonParser": {
        "timestampField": "timestamp",
        "columns": {
            "autocheck_config": {
                "type": "String",
                "jsonPath": "value.payload.autocheck_config"
            },
            "circuit_type": {
                "type": "String",
                "jsonPath": "value.payload.circuit_type"
            },
            "is_fallback": {
                "type": "UInt8",
                "jsonPath": "value.payload.is_fallback"
            },
            "gg_cache_is_found": {
                "type": "UInt8",
                "jsonPath": "value.payload.gg_cache_is_found"
            },
            "is_precommit": {
                "type": "UInt8",
                "jsonPath": "value.payload.is_precommit",
                "defaultValue": 0
            },
            "recheck": {
                "type": "UInt8",
                "jsonPath": "value.payload.recheck",
                "defaultValue": 0
            },
            "left": {
                "type": "String",
                "jsonPath": "value.payload.left"
            },
            "right": {
                "type": "String",
                "jsonPath": "value.payload.right"
            },
            "raw_value": {
                "type": "String",
                "field": "value"
            }
        }
    }
}
