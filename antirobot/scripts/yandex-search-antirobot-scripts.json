{
    "meta": {
        "name": "yandex-search-antirobot-scripts",
        "maintainer": "Dmitry Spasskiy <thatguy@yandex-team.ru>",
        "description": "Supporting scripts for Antirobot",
        "version": "{revision}",
        "depends": ["yandex-logmux-collector-ng-stable"]
    },
    "build": {
        "targets": [
            "antirobot/scripts/support"
        ]
    },
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/scripts/support"
            },
            "destination": {
                "path": "/hol/antirobot/scripts",
                "attributes": {
                    "user": {
                        "value": "root",
                        "recursive": true
                    },
                    "group": {
                        "value": "root",
                        "recursive": true
                    }
                }
            }
        }
    ]
}