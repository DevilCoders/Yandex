        "name": "yandex-device_validator-bundle",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "device_validator_bundle package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/device_validator/build"
        ]
    },
    "data": [
