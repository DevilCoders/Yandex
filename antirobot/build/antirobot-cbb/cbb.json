{
    "meta": {
        "name": "yandex-antirobot-cbb_django",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "cbb package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/build/antirobot-cbb"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "antirobot/build/antirobot-cbb",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/cbb/cbb_django/static",
                "files": ["*"]
            },
            "destination": {
                "path": "/static/"
            }
        },
        {
            "source": {
                "type": "SANDBOX_RESOURCE",
                "id": 225296080,
                "path": "memcached"
            },
            "destination": {
                "path": "/memcached",
                "attributes": {
                    "mode": {
                        "value": "+x"
                    }
                }
            }
        }
    ]
}
