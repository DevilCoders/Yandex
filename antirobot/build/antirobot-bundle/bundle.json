{
    "meta": {
        "name": "yandex-antirobot-bundle",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "antirbot_bundle package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/build/antirobot-bundle"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "antirobot/build/antirobot-bundle",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/config",
                "files": ["*.json"]
            },
            "destination": {
                "path": "/arcadia_config/"
            }
        }
    ]
}
