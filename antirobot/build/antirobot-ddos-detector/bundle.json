{
    "meta": {
        "name": "yandex-antirobot-ddos-detector",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "antirbot_ddos_detector package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/build/antirobot-ddos-detector"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "antirobot/build/antirobot-ddos-detector",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/tools/get_rps",
                "files": ["config.yml"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
