{
    "meta": {
        "name": "yandex-captcha_demo-bundle",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "captcha_demo_bundle package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/captcha/ext_captcha_demo/build"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "antirobot/captcha/ext_captcha_demo/build",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/captcha/ext_captcha_demo/bin/templates",
                "files": ["*"]
            },
            "destination": {
                "path": "/templates/"
            }
        }
    ]
}

