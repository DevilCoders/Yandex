{
    "meta": {
        "name": "yandex-fingerprint-bundle",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "fingerprint_bundle package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/captcha/fingerprint/build"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "antirobot/captcha/fingerprint/build",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/captcha/fingerprint/server/generated",
                "files": ["*"]
            },
            "destination": {
                "path": "/generated/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "antirobot/captcha/fingerprint/server/templates",
                "files": ["*"]
            },
            "destination": {
                "path": "/templates/"
            }
        }
    ]
}

