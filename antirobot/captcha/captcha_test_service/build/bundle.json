{
    "meta": {
        "name": "yandex-captcha_test_service-bundle",
        "maintainer": "antirobot@yandex-team.ru",
        "description": "captcha_test_service_bundle package",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "antirobot/captcha/captcha_test_service/build"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "antirobot/captcha/captcha_test_service/build",
                "files": ["*"]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}

