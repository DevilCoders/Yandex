{
  "logging": {
    "file": "/var/log/solomon_sender/solomon_sender.log",
    "level": "DEBUG",
    "file_size_bytes": 30000000,
    "backup_count": 10
  },
  "port": 42000,
  "backup_settings": {
    "on": true,
    "file": "/tmp/graphite-to-solomon-sender/solomon_sender_backup.bin",
    "delay_millis":60000
  },
  "buffer": {
    "length": 3000000,
    "check_health_delay_millis":1000
  },
  "solomon_http_sender": {
    "on": {{solomon_enable}},
    "delay_millis":1000,
    "sender_id": "s1",
    "url": "http://solomon.yandex.net/api/v2/push",
    "oauth_token": "{{oauth}}",
    "read_timeout_millis": 10000,
    "connect_timeout_millis": 12323,
    "project": "{{project}}",
    "cluster": "{{cluster_name}}",
    "service": "{{service}}"
  },
  "graphite_senders": [
    {
      "on": true,
      "delay_millis": 1000,
      "metric_prefix": "",
      "max_batch_size": 5000,
      "sender_id": "g1",
      "server": "gr.mega-graphite-sas.search.yandex.net",
      "port": 2024
    },
    {
      "on": true,
      "delay_millis": 1000,
      "metric_prefix": "",
      "max_batch_size": 5000,
      "sender_id": "g2",
      "server": "gr.mega-graphite-man.search.yandex.net",
      "port": 2024
    }
  ],
  "health_reporter": {
    "on": true,
    "delay_millis":1000,
    "file": "/tmp/graphite-to-solomon-sender/healthcheck.txt"
  },
  "blocking_log_threshold_sec":5,
  "blocking_signal_threshold_sec":10
}
