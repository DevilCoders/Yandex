/var/log/mds/tskv.log
/var/log/mds/thevoid.log
/var/log/mds/access.log
/var/log/mds/mds-access.log
/var/log/mds/elliptics.log
{
    daily
    maxsize 5G
    missingok
    rotate 30
    compress
    compresscmd /usr/bin/ionice
    compressoptions -c3 /usr/bin/pzstd -1 -p2
    compressext .zst
    delaycompress
    notifempty
    dateext
    dateformat .%Y%m%d-%s
    nocreate
    postrotate
        kill -HUP `pidof mediastorage-proxy`
    endscript
}
