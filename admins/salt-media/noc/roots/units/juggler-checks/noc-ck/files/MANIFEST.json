{
    "version": 1,
    "env": {"CONFIG": "/etc/noc-ck/config.yml"},
    "checks": [
        {
            "check_script": "/usr/share/python3/venvs/noc-ck/bin/noc-ck-monitor-elections",
            "interval": 60,
            "run_always": true,
            "services": [
                "noc-ck-elections"
            ],
            "timeout": 30
        },
        {
            "check_script": "/usr/share/python3/venvs/noc-ck/bin/noc-ck-device-locks",
            "interval": 60,
            "run_always": true,
            "services": [
                "noc-ck-device-lock"
            ],
            "timeout": 30
        }
    ]
}
