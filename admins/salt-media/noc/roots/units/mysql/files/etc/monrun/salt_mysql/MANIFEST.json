{
    "version": 1,
    "checks": [{
        "check_script": "/usr/sbin/mysql-status.sh",
        "interval": 30,
        "run_always": true,
        "services": [
            "mysql_status"
        ],
        "timeout": 10
    },
    {
        "check_script": "/usr/sbin/mysync-maint.sh",
        "interval": 30,
        "run_always": true,
        "services": [
            "mysql_mysync_status"
        ],
        "timeout": 10
    }]
}
