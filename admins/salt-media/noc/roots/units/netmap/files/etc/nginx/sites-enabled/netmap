server {
  listen          443;
  listen          [::]:443;

  server_name netmap.net.yandex.net default_server;
  client_max_body_size  64M;
  keepalive_timeout 5;
  proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
  proxy_set_header Host $http_host;
  proxy_redirect off;

  ssl_certificate_key "/etc/nginx/ssl/netmap.net.yandex.net.key";
  ssl_certificate "/etc/nginx/ssl/netmap.net.yandex.net.crt";

  location /export/netmap/ {
    access_log off;
    autoindex on;
    root /www/racktables;
  }
}
