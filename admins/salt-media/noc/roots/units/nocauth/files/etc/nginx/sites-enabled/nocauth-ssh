server {
  listen          80;
  listen          [::]:80;

  server_name nocauth-ssh.yandex.net default_server;
  client_max_body_size  64M;
  keepalive_timeout 5;
  proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
  proxy_set_header Host $http_host;
  proxy_redirect off;

  # ssh
  location /ssh/ {
    rewrite ^/ssh(.*)$ $1 break;
    proxy_pass http://localhost:6688;
  }
}
