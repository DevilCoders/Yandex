nginx=perl -e '@t=(); %c=(2=>0,3=>0,4=>0,5=>0); while(<>){ /^[^"]+"[^"]+" (\d+).* ([\d\.]+)\s+\S+\s+\d+$/ or next; { use integer; $c{$1/100}++; } if($1==200){ push @t,$2; } } foreach(keys %c){ print "status.${_}xx $c{$_}\n" } print "request_timings @t\n";'

mail_sent=grep "status=sent (250.*Ok" |wc -l |awk '{print "mail_ok " $1}'

nginx_tskv=tskv -h ".kinopoisk.ru" --timings-code-filter ^2 -y -s -t req -z
tinyproxy_not_200=grep 'Unable to download page thought the proxy' | egrep -o 'code:.*' | awk -F '"' '{ print $1 }' | sort | uniq -c | awk {' print $3" "$1'}
