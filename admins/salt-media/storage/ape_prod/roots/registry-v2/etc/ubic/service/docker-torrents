use Ubic::Service::SimpleDaemonTorrents;
my $service = Ubic::Service::SimpleDaemonTorrents->new(
    name => 'docker-torrents',
    auto_start => 1,
    ubic_log => '/var/log/ubic/docker-torrents.ubic.log',
    stdout => '/var/log/ubic/docker-torrents.stdout.log',
    stderr => '/var/log/ubic/docker-torrents.stderr.log',
    bin => 'python /usr/bin/docker-registry-torrents -c /etc/docker-registry-torrents/config.yaml',
);

