use Ubic::Service::SimpleDaemon;
my $service = Ubic::Service::SimpleDaemon->new(
    name => 'docker-distribution',
    auto_start => 1,
    ubic_log => '/var/log/ubic/docker-distribution.ubic.log',
    stdout => '/var/log/ubic/docker-distribution.stdout.log',
    stderr => '/var/log/ubic/docker-distribution.stderr.log',
    bin => '/usr/bin/registry serve /etc/docker-distribution.yml',
);

