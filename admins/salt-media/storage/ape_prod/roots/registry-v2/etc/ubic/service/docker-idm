use Ubic::Service::SimpleDaemon;
my $service = Ubic::Service::SimpleDaemon->new(
    name => 'docker-idm',
    auto_start => 1,
    ubic_log => '/var/log/ubic/docker-idm.ubic.log',
    stdout => '/var/log/ubic/docker-idm.stdout.log',
    stderr => '/var/log/ubic/docker-idm.stderr.log',
    bin => '/usr/bin/docker-idm-server -config /etc/docker-idm.json',
);

