/var/log/nginx/access_tskv.log  {
#
# 'maxsize' only valid for yandex-logrotate package
#        maxsize 500M
        daily
        rotate 10
        compress
        dateext
        dateformat -%Y%m%d-%s
        missingok
        notifempty
        postrotate
                /etc/init.d/nginx reload > /dev/null 2>&1;
        endscript
}

