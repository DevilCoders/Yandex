/var/log/nginx/strm-tskv.log
{
    maxsize 2048M
    daily
    missingok
    rotate 7
    compress
    compresscmd /usr/bin/pzstd
    compressoptions -8 -p1
    compressext .zst
    delaycompress
    notifempty
    dateext
    dateformat .%Y%m%d-%s
    sharedscripts
    postrotate
            kill -USR1 `cat /var/run/nginx.pid` && sleep 2
    endscript
}
