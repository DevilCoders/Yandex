{
    "module": "Ubic::Service::PlayList",
    "options": {
        "listen_port": {{ playlist_port }},
        "timeout_options": {
            "start": {
                "step": 1,
                "trials": 3
            },
            "ping": 2
        },
        "user": "root",
        "daemon_user": "www-data",
        "ulimit": {
            "RLIMIT_NOFILE": 65535,
            "RLIMIT_CORE": -1
        }
    }
}
