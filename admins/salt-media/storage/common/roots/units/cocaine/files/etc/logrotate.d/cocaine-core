/var/log/cocaine-core/cocaine-tskv.log {
    daily
    maxsize 4G
    rotate 5
    delaycompress
    compress
    missingok
    notifempty
    dateext
    dateformat .%Y%m%d-%s
    nocreate
    postrotate
        pkill -HUP -Ucocaine cocaine-runtime
    endscript
}


