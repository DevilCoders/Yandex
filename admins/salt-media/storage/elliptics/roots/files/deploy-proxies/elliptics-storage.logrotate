/var/log/elliptics/node*.log
/var/log/elliptics/tskv.log
{
    daily
    compress
    nocreate
    dateext
    dateformat -%Y%m%d-%s
    missingok
    rotate 10
    maxsize 30G
    postrotate
        kill -HUP `pidof dnet_ioserv` > /dev/null 2>&1 || true
    endscript

}

/var/log/elliptics/ubic*.log
{
    daily
    compress
    copytruncate
    dateext
    dateformat -%Y%m%d-%s
    missingok
    rotate 15
}
