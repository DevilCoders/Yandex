ssl_certificate     /etc/yandex-certs/mds.yandex.net.pem;
ssl_certificate_key /etc/yandex-certs/mds.yandex.net.key;
ssl_prefer_server_ciphers on;
ssl_protocols TLSv1 TLSv1.1 TLSv1.2 TLSv1.3;
ssl_ciphers kEECDH+AESGCM+AES128:kEECDH+AES128:kRSA+AESGCM+AES128:kRSA+AES128:!aNULL:!eNULL:!MD5:!EXPORT:!LOW:!SEED:!CAMELLIA:!IDEA:!PSK:!SRP:!SSLv2;
ssl_session_cache    shared:MDS_SSL_CACHE:128m;
ssl_session_timeout  28h;

resolver 127.0.0.1 141.8.146.1;
ssl_stapling on;
ssl_stapling_verify on;
ssl_trusted_certificate /etc/nginx/ssl/certuml4.pem;
