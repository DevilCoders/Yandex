/var/log/collect_buckets_script.log
{
    missingok
    maxsize 1G
    rotate 7
    compress
    compresscmd /usr/bin/ionice
    compressoptions -c3 /usr/bin/pzstd -1 -p2
    compressext .zst
    delaycompress
    daily
    notifempty
    create 644 root adm
    dateext
    dateformat .%Y%m%d-%s
}

