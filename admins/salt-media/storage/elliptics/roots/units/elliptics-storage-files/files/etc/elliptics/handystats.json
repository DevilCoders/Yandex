{
    "io.cmds": {
        "tags": ["rate", "value"]
    },
    "io.cmds.*": {
        "tags": ["rate", "value"]
    },

    "io.cmd.*.*": {
        "tags": ["value"]
    },

    "io.cmd.*": {
        "tags": ["count", "moving-count", "histogram"]
    },

    "io.{input,output}.queue.size": {
        "tags": ["value", "moving-avg"]
    },

    "io.disconnects.*": {
        "tags": ["value"]
    },

    "pool.*.{blocking,nonblocking,lifo}.queue.size": {
        "tags": ["value", "moving-avg"]
    },

    "eblob.*.disk.stat_commit.errors.*": {
        "tags": ["value", "count"]
    },

    "eblob.*.remove.size": {
        "tags": ["value"]
    },

    "eblob.*.verify_checksum.*": {
        "tags": ["value"]
    },

    "backend.*.{stamp_corruption,marked_corrupted}": {
        "tags": ["value"]
    },

    "pool.*.queue.dropped": {
        "tags": ["value", "moving-count"]
    },

    "enable": true,
    "dump-interval": 1000,
    "defaults": {
        "moving-interval": 5000,
        "rate-unit": "s",
        "tags": []
    },
    "timer": {
        "histogram-bins": 30
    }
}
