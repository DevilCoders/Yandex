{
    "module": "Ubic::Service::ResourceProvider",
    "options": {
        "user": "root",
        "rlimit_core": -1,
        "config": "/etc/nscfg/resource-provider.conf",
        "log_dir": "/var/log/resource-provider",
        "timeout": 2,
        "start_attempts": 5,
        "ping_attempts": 10,
        "stop_timeout": 30,
        "check_timeout": 20
    }
}
