use Ubic::Service::SimpleDaemon;
my $service = Ubic::Service::SimpleDaemon->new(
    name => 'nscfg-server-ng',
    bin => '/usr/bin/nscfg-next-gen-server -c /etc/elliptics/nscfg-server-ng.conf',
    auto_start => 1,
    ubic_log => '/var/log/nscfg-server-ng/ubic.log',
    stdout => '/var/log/nscfg-server-ng/ubic-stdout.log',
    stderr => '/var/log/nscfg-server-ng/ubic-stderr.log',
);
