use Ubic::Service::SimpleDaemon;
my $service = Ubic::Service::SimpleDaemon->new(
    name => 'nscfg-server',
    bin => '/usr/bin/nscfg-server -c /etc/elliptics/nscfg-server.conf',
    auto_start => 1,
    ubic_log => '/var/log/nscfg-server/ubic.log',
    stdout => '/var/log/nscfg-server/ubic-stdout.log',
    stderr => '/var/log/nscfg-server/ubic-stderr.log',
);
