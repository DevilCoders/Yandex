{
  "sources": [
      "logbroker://cocaine-backend--cocaine-log"
  ],
  "logHosts": "*",
  "logPath": "/var/log/logshatter/logshatter-lb-response.log",
  "parser": {
    "dateFormat": "dd/MMM/yyyy:HH:mm:ss Z",
    "separator": "\t",
    "fields": [
      "date", "method", "http_code", "resptime_ms", "lb_host", "session", "response_size_byte", "messages_count"
    ],
    "columns": {
      "host": {
        "type": "String"
      },
      "method": {
        "type": "String"
      },
      "http_code": {
        "type": "UInt16"
      },
      "resptime_ms": {
        "type": "UInt32"
      },
      "lb_host": {
        "type": "String"
      },
      "response_size_byte": {
        "type": "UInt32"
      },
      "messages_count": {
        "type": "UInt32"
      }
    }
  },
  "clickhouseTable": "logshatter_lb_response"
}
