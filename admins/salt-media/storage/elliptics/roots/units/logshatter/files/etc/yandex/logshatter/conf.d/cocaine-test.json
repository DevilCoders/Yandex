{
    "clickhouseTable": "cocaine_test_logs",
    "dataRotationDays": 14,
    "sources": [
        "logbroker://cocaine-test-backend--cocaine-log",
        "logbroker://cocaine-test-front--cocaine-log"
    ],
    "logHosts": "*",
    "logPath": "{/var/log/nginx/access_tskv.log,/var/log/cocaine-core/cocaine-tskv.log,/var/log/cocaine-runtime/cocaine.log,/var/log/cocaine-runtime/core.log,/var/log/cocaine-core/cocaine-logging-tskv.log,/var/log/cocaine-core/cocaine-tskv.log}",
    "parser": {
        "separator": "\t",
        "timestampFormat": "UNIXTIME_DOT_MICROS",
        "tskvMatch": {
            "request": "message",
            "request_id": "trace_id",
            "source": "app",
            "type": "source_type",
            "unixtime_microsec_utc": "timestamp"
        },
        "columns": {
            "app": {
                "default": "",
                "type": "String"
            },
            "error_generated_by": {
                "default": "",
                "type": "String"
            },
            "host": {
                "type": "String"
            },
            "ip": {
                "default": "",
                "type": "String"
            },
            "message": {
                "default": "",
                "type": "String"
            },
            "method": {
                "default": "",
                "type": "String"
            },
            "parent_id": {
                "default": "",
                "type": "String"
            },
            "pid": {
                "default": 0,
                "type": "UInt32"
            },
            "request_length": {
                "default": "",
                "type": "String"
            },
            "request_time": {
                "default": -1,
                "type": "Float64"
            },
            "rpc_name": {
                "default": "",
                "type": "String"
            },
            "severity": {
                "default": "",
                "type": "String"
            },
            "source_type": {
                "default": "",
                "type": "String"
            },
            "span_id": {
                "default": "",
                "type": "String"
            },
            "ssl_handshake_time": {
                "default": "",
                "type": "String"
            },
            "status": {
                "default": 0,
                "type": "UInt16"
            },
            "tid": {
                "default": "",
                "type": "String"
            },
            "timestamp_micros": {
                "default": 0,
                "type": "UInt64"
            },
            "trace_id": {
                "default": "",
                "type": "String"
            },
            "upstream_cache_status": {
                "default": "",
                "type": "String"
            },
            "upstream_response_time": {
                "default": "",
                "type": "String"
            },
            "uuid": {
                "default": "",
                "type": "String"
            },
            "vhost": {
                "default": "",
                "type": "String"
            }
        }
    }
}
