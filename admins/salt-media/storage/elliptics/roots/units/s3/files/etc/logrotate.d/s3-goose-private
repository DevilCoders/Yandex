/var/log/s3/goose-private/*.log {
    compress
    compresscmd /usr/bin/ionice
    compressext .zst
    compressoptions -c3 /usr/bin/pzstd -1 -p2
    create 644 s3proxy s3proxy
    dateext
    dateformat .%Y%m%d-%s
    maxsize 500M
    missingok
    nocreate
    notifempty
    rotate 24
    postrotate
        kill -HUP `pidof s3-goose-private`
    endscript
}
