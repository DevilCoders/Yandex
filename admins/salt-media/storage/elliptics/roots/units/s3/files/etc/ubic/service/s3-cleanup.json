{
    "module": "Ubic::Service::S3Cleanup",
    "options": {
        "config": "/etc/s3-goose/main-cleanup.json",
        "timeout_options": {
            "start": {
                "step": 1,
                "trials": 3
            },
            "ping": {
                "timeout": 60,
                "attempts": 10
            }
        },
        "user": "root",
        "daemon_user": "s3proxy",
        "ulimit": {
            "RLIMIT_NOFILE": 65535,
            "RLIMIT_CORE": -1
        },
        "ubic_log": "/var/log/s3/goose-cleanup/ubic.log",
        "stdout": "/var/log/s3/goose-cleanup/ubic-stdout.log",
        "stderr": "/var/log/s3/goose-cleanup/ubic-stderr.log"
    }
}
