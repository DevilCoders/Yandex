{
    "module": "Ubic::Service::S3Billing",
    "options": {
        "config": "/etc/s3-goose/main-billing.json",
        "timeout_options": {
            "start": {
                "step": 1,
                "trials": 6
            },
            "ping": {
                "timeout": 5,
                "attempts": 3
            }
        },
        "run_dir": "/var/run/s3-billing",
        "user": "root",
        "daemon_user": "s3proxy",
        "ulimit": {
            "RLIMIT_NOFILE": 65535,
            "RLIMIT_CORE": -1
        },
        "env": {
            "GOTRACEBACK": "crash"
        },
        "ubic_log": "/var/log/s3/billing/ubic.log",
        "stdout": "/var/log/s3/billing/ubic-stdout.log",
        "stderr": "/var/log/s3/billing/ubic-stderr.log"
    }
}
