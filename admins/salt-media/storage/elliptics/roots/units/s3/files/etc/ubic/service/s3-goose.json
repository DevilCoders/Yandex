{
    "module": "Ubic::Service::S3Goose",
    "options": {
        "config": "/etc/s3-goose/main-goose.json",
        "timeout_options": {
            "start": {
                "step": 1,
                "trials": 6
            },
            "ping": {
                "timeout": 5,
                "attempts": 3
            }
        },
        "check_addr": "http://localhost:3355/",
        "check_uri": "/ping",
        "run_dir": "/var/run/s3-goose",
        "socket_perms": "0775",
        "socket_owners": ":www-data",
        "user": "root",
        "daemon_user": "s3proxy",
        "ulimit": {
            "RLIMIT_NOFILE": 65535,
            "RLIMIT_CORE": -1
        },
        "env": {
            "GODEBUG": "madvdontneed=1",
            "GOTRACEBACK": "crash"
        },
        "ubic_log": "/var/log/s3/goose/ubic.log",
        "stdout": "/var/log/s3/goose/ubic-stdout.log",
        "stderr": "/var/log/s3/goose/ubic-stderr.log"
    }
}
