{
    "logging": {
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": {
            "default": {
                "()": "s3mds.logger.BaseFormatter",
                "format": "%(asctime)s.%(msecs)03d %(levelname)s: %(message)s",
                "datefmt": "%Y-%m-%dT%H:%M:%S"
            }
        },
        "handlers": {
            "default": {
                "formatter": "default",
                "level": "DEBUG",
                "class": "logging.handlers.WatchedFileHandler",
                "filename": "/var/log/s3-dispenser/s3-dispenser.log"
            }
        },
        "loggers": {
            "": {
                "handlers": [
                    "default"
                ],
                "level": "DEBUG",
                "propagate": true
            }
        }
    },
    "dispenser": {
        "base_url": "https://dispenser.test.yandex-team.ru",
        "oauth_token": "{{ pillar['yav']['s3-dispenser-oauth-token'] }}"
    },
    "service_stats": {
        "url": "https://s3-idm.mdst.yandex.net/stats/services/"
    }
}

