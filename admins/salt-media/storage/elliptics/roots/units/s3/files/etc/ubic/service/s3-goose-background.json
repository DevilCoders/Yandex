{
    "module": "Ubic::Service::S3Background",
    "options": {
        "config": "/etc/s3-goose/main-background.json",
        "timeout_options": {
            "start": {
                "step": 1,
                "trials": 3
            },
            "ping": {
                "timeout": 8,
                "attempts": 2
            }
        },
        "user": "root",
        "daemon_user": "s3proxy",
        "ulimit": {
            "RLIMIT_NOFILE": 65535,
            "RLIMIT_CORE": -1
        },
        "env": {
            "GOTRACEBACK": "crash"
        },
        "ubic_log": "/var/log/s3/goose-background/ubic.log",
        "stdout": "/var/log/s3/goose-background/ubic-stdout.log",
        "stderr": "/var/log/s3/goose-background/ubic-stderr.log"
    }
}
