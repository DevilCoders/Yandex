/var/log/s3/billing/*.log
{
    compress
    compresscmd /usr/bin/ionice
    compressext .zst
    compressoptions -c3 /usr/bin/pzstd -1 -p2
    create 644 s3proxy s3proxy
    dateext
    dateformat .%Y%m%d-%s
    missingok
    nocreate
    notifempty
    rotate 24
    size 500M
    postrotate
        kill -HUP `pidof s3-goose-billing`
    endscript
}

