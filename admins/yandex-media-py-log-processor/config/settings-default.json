{
  "log_path": "/var/log/nginx/access.log",
  "depth_seconds": 120,
  "global_filter": {
    "include": [
      ".*"
    ],
    "exclude": [
      ".*ping.*"
    ]
  },
  "log_monitor_settings": [
    {
      "name": "5xx rate",
      "filter": {
        "include": [
          "status=5"
        ],
        "exclude": []
      },
      "alert_configuration": {
        "type": "rate",
        "warning": 0.005,
        "critical": 0.01
      }
    },
    {
      "name": "499 rate",
      "filter": {
        "include": [
          "status=499"
        ],
        "exclude": []
      },
      "alert_configuration": {
        "type": "rate",
        "warning": 0.005,
        "critical": 0.01
      }
    },
    {
      "name": "percentile",
      "filter": {
        "include": [
          ".*"
        ],
        "exclude": ["/\\w+/regions/\\d+/search"]
      },
      "alert_configuration": {
        "type": "percentile",
        "percentile": 100,
        "warning": 0.4,
        "critical": 0.6
      }
    }
  ]
}
