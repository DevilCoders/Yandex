{
    "meta": {
        "name": "go-zk-client",
        "maintainer": "Sergey Kacheev <skacheev@yandex-team.ru>",
        "description": "zk is a command line client to the Zookeeper distributed storage service",
        "version": "{changelog_version}.{revision}.{branch}",
        "noconffiles_all": true,
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/admins/zk/"
    },
    "build": {
        "zk_build": {
            "targets": ["admins/zk/cmd"]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "zk_build",
                "path": "admins/zk/cmd/zk"
            },
            "destination": {
                "path": "/usr/bin/"
            }
        }
    ]
}
