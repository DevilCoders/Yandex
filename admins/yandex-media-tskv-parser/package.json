{
    "meta": {
        "name": "yandex-media-tskv-parser",
        "maintainer": "Sergey Kacheev <skacheev@yandex-team.ru>",
        "description": "tskv nginx log parser and pretty printer. Also provide cool utility to make tskv human readable and choice fileds from tskv to display",
        "version": "{changelog_version}",
        "noconffiles_all": true,
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/admins/yandex-media-tskv-parser",
        "depends": [
            "mawk (>= 1.9.9.6-yandex1)",
            "yandex-timetail"
        ]
    },
    "build": {
        "gskv_build": {
            "targets": [
                "music/tools/toolbox/tools/gskv/cmd"
            ]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "gskv_build",
                "path": "music/tools/toolbox/tools/gskv/cmd/gskv"
            },
            "destination": {
                "path": "/usr/sbin/"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "admins/yandex-media-tskv-parser/sbin",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/usr/sbin/",
                "attributes": {
                    "mode": {
                        "value": "+x"
                    }
                }
            }
        }
    ]
}
