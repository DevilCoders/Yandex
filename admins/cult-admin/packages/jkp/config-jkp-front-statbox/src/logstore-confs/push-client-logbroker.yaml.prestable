---
ident: jkp-front
network:
  proto: rt
  master_addr: logbroker.yandex.net
  data-port: 9000

files:
  - name: /var/log/nginx/access.tskv.log
    log_type: access-log

logger:
  timeformat: "[%d/%b/%Y:%T %z]"
  file: /var/log/statbox/logbroker.log

watcher:
  state: /var/lib/push-client/state-logbroker
