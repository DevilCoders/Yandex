#!/usr/bin/make -f

%:
	dh $@

binary:
	dh binary --until dh_install

	./install-package.sh debian/yandex-zookeeper-jkp zookeeper-jkp yandex-zookeeper-jkp jkp

	dh binary --after dh_install

override_dh_installinit:
	dh_installinit --name=yandex-zookeeper-jkp --no-start --no-restart-on-upgrade
	dh_installinit --name=yandex-zookeeper-rest-jkp --no-start --no-restart-on-upgrade

# vim: set ts=8 sw=8 noet:
