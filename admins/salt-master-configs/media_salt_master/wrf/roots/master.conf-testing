# main master config
interface: '::'
ipv6: True
worker_threads: 12
user: robot-weather-admin
top_file_merging_strategy: merge
fileserver_backend:
  - roots
file_roots:
  testing:
    - /srv/git/common/roots/
    - /srv/git/wrf/roots/
    - /srv/salt/roots/
pillar_roots:
  testing:
    - /srv/git/common/pillar/
    - /srv/git/wrf/pillar/
log_level: INFO
log_level_logfile: INFO
renderer: jinja | yaml | gpg
auto_accept: True
ping_on_rotate: True
keep_jobs: 1

