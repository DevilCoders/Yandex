{
    "meta": {
        "name": "yandex-lazy-trim",
        "maintainer": "Yandex.Disk <disk-admin@yandex-team.ru>",
        "description": "Small utility for throtted trimming ssd disks",
        "version": "{changelog_version}",
        "depends": ["base-files"],
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/admins/lazy_trim"
    },
    "build": {
        "targets": [
            "admins/lazy_trim"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "admins/lazy_trim/lazy-trim"
            },
            "destination": {
                "path": "/usr/bin/lazy-trim",
                "attributes": {
                    "mode": {
                        "value": "0755"
                    }
                }
            }
        }
    ]
}
