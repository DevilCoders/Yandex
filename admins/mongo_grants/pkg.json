{
    "meta": {
        "name": "corba-mongo-grants",
        "maintainer": "Sergey Kacheev <skacheev@yandex-team.ru>",
        "description": "https://wiki.yandex-team.ru/geo-admin/corba-mongo-grants mongo grants generator",
        "version": "{changelog_version}.{revision}.{branch}",
        "noconffiles_all": true,
        "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/admins/mongo_grants/"
    },
    "build": {
        "mongo_grants_build": {
            "targets": ["admins/mongo_grants"]
        }
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "mongo_grants_build",
