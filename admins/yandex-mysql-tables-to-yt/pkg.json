{
    "meta": {
        "name": "yandex-mysql-tables-to-yt",
        "maintainer": "Ilya Karpov <gibzer@yandex-team.ru>",
        "description": "Upload MySQL tables to YT",
        "version": "{changelog_version}",
        "depends": ["yandex-yt-python", "libipc-run-perl", "zstd"],
        "homepage": "https://a.yandex-team.ru/arc_vcs/admins/yandex-mysql-tables-to-yt"
    },
    "build": {
        "targets": [
            "admins/yandex-mysql-tables-to-yt"
        ]
    },
    "data": [
        {
            "source": {
                "type": "ARCADIA",
                "path": "admins/yandex-mysql-tables-to-yt/src/usr/bin"
            },
            "destination": {
                "path": "/usr/bin"
            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "admins/yandex-mysql-tables-to-yt/src/etc/yandex"
            },
            "destination": {
                "path": "/etc/yandex"
            }
        }
    ]
}
