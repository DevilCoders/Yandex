{
  "meta": {
    "name": "repo-upstream-salt",
    "maintainer": "NOCDEV <noc-dev@yandex-team.ru>",
    "description": "Upstream Salt Repo",
    "homepage": "https://a.yandex-team.ru/arc/trunk/arcadia/admins/repo-upstream-salt",
    "noconffiles_all": true,
    "version": "0.1"
  },
  "build": {},
  "data": [
    {
        "source": {
            "type": "ARCADIA",
            "path": "admins/repo-upstream-salt/salt-upstream-archive-keyring.gpg"
        },
        "destination": {
            "path": "/etc/apt/trusted.gpg.d/",
            "attributes": {
                "owner": {
                    "value": "root"
                },
                "mode": {
                    "value": "-w"
                }
            }
        }
    },
    {
        "source": {
            "type": "ARCADIA",
            "path": "admins/repo-upstream-salt",
            "files": ["glob:*.list"]
        },
        "destination": {
            "path": "/usr/share/repo-upstream-salt/"
        }
    }
  ]
}
