#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-monitoring-corba-cback/etc/monrun/conf.d/
	mkdir -p debian/config-monitoring-corba-cback/usr/bin/
	cp -r monrun-conf.d/* debian/config-monitoring-corba-cback/etc/monrun/conf.d/
	cp -r bin/* debian/config-monitoring-corba-cback/usr/bin/

%:
	dh $@

