#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-corba-pcback/
	rm -fr debian/config-corba-pcback/*
	mkdir -p debian/config-corba-pcback/etc/logster/
	mkdir -p debian/config-corba-pcback/etc/cron.d/

	cp logster/logster.conf	debian/config-corba-pcback/etc/logster/logster.conf
	cp cron.d/logster debian/config-corba-pcback/etc/cron.d/logster

%:
	dh $@

