#!/bin/bash

set -e

case "$1" in
    purge)
        rm -rf /etc/xscript-multiple/conf-enabled/yandex-team
        rm -rf /var/cache/xscript-multiple/yandex-team
        rm -rf /var/run/xscript-multiple/yandex-team
        rm -rf /tmp/xscript-multiple/yandex-team
    ;;

    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
