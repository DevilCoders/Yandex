#!/bin/bash
# postinst script for yandex-corba-export-vhost
#
# see: dh_installdeb(1)

set -e

# summary of how this script can be called:
#        * <postinst> `configure' <most-recently-configured-version>
#        * <old-postinst> `abort-upgrade' <new version>
#        * <conflictor's-postinst> `abort-remove' `in-favour' <package>
#          <new-version>
#        * <postinst> `abort-remove'
#        * <deconfigured's-postinst> `abort-deconfigure' `in-favour'
#          <failed-install-package> <version> `removing'
#          <conflicting-package> <version>
# for details, see http://www.debian.org/doc/debian-policy/ or
# the debian-policy package


case "$1" in
    configure)

    lighty-x5-enable-mod redirects
    lighty-x5-enable-mod export
    lighty-x5-enable-mod xscript
    lighty-x5-enable-mod geobase
    lighty-x5-enable-mod warhammer
    lighty-x5-enable-mod soft-export
    lighty-x5-enable-mod yaca
    lighty-x5-enable-mod pda-yaca
    lighty-x5-enable-mod forum-direct
    lighty-x5-enable-mod widgets
    lighty-x5-enable-mod sendform
    lighty-x5-enable-mod site
    lighty-x5-enable-mod muzei
    lighty-x5-enable-mod nahodki
    lighty-x5-enable-mod terms
    lighty-x5-enable-mod ie
    lighty-x5-enable-mod bar
    lighty-x5-enable-mod adresa-mobile
    lighty-x5-enable-mod referats
    lighty-x5-enable-mod promo-money
    lighty-x5-enable-mod lenta-ng
    lighty-x5-enable-mod server
    lighty-x5-enable-mod icq
    lighty-x5-enable-mod interests
    lighty-x5-enable-mod wy
    lighty-x5-enable-mod local
    lighty-x5-enable-mod online
    lighty-x5-enable-mod yaprobki
    lighty-x5-enable-mod punto
    lighty-x5-enable-mod collection
    lighty-x5-enable-mod xml
    lighty-x5-enable-mod lost
    lighty-x5-enable-mod feedback
    lighty-x5-enable-mod adresa
    lighty-x5-enable-mod firefox
    lighty-x5-enable-mod chromium
    lighty-x5-enable-mod research
    lighty-x5-enable-mod webmaster
    lighty-x5-disable-mod partner-news
    lighty-x5-enable-mod soft
    lighty-x5-enable-mod adv
    lighty-x5-enable-mod tune
    lighty-x5-enable-mod play
    lighty-x5-enable-mod wt
    lighty-x5-enable-mod pda-lenta
    lighty-x5-enable-mod 2010
    lighty-x5-enable-mod desktop
    lighty-x5-enable-mod kubok-export
    lighty-x5-enable-mod search-ruscorpora
    lighty-x5-enable-mod mobile-feedback
    lighty-x5-enable-mod rules
    lighty-x5-enable-mod cards
    lighty-x5-enable-mod kraski
    lighty-x5-enable-mod bookmarks
    lighty-x5-enable-mod company-com
    lighty-x5-enable-mod crossdomain-xml
    lighty-x5-enable-mod bookmarks-mobile
    lighty-x5-enable-mod xsl-lego-deny
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

exit 0


