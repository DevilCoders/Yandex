upstream parking_upstream {
        server parking01f.tools.yandex.net:8080 max_fails=1 fail_timeout=20s;
}

server {
  listen 80;
  listen 443;

  server_name parking.*;

  access_log /var/log/nginx/parking.access.log main;

  location / {
			
    proxy_pass http://parking_upstream;
    proxy_set_header Host                   $host;
    proxy_set_header X-Real-IP              $remote_addr;
  }
}
