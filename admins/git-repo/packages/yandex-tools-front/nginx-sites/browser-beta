server {
       listen 443;
       server_name browser-beta.yandex-team.ru;
       location / {
               proxy_set_header X-Real-IP $remote_addr;
               proxy_set_header Host browser.ichthys.user.daze.yandex.ru;
       	       proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
               proxy_pass http://daze.yandex.ru;
       }
}

