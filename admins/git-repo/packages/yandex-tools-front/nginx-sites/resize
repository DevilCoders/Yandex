upstream resize_upstream {
#	server resize01e.tools.yandex.net max_fails=1 fail_timeout=10s;
        server resize01g.tools.yandex.net max_fails=1 fail_timeout=10s;
}

server {
	listen 80;
	listen 443;

	server_name  resize.*;

	access_log /var/log/nginx/resize.access.log main;

	location / {
		proxy_pass http://resize_upstream;
		proxy_set_header Host                   resize.yandex-team.ru;
		proxy_set_header X-Real-IP              $remote_addr;
	}
}
