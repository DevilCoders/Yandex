server {
        listen 80;
        listen 443;

	server_name  study.yandex-team.ru;


        access_log /var/log/nginx/events.access.log main;

        location / {
                proxy_pass       https://93.158.157.211;
                proxy_set_header Host      study.yandex-team.ru;
                proxy_set_header X-Real-IP $remote_addr;
        }
}

