#!/bin/sh
# postinst script for yandex-corba-export-vhost-ng
#
# see: dh_installdeb(1)

set -e

# summary of how this script can be called:
#        * <postinst> `configure' <most-recently-configured-version>
#        * <old-postinst> `abort-upgrade' <new version>
#        * <conflictor's-postinst> `abort-remove' `in-favour' <package>
#          <new-version>
#        * <postinst> `abort-remove'
#        * <deconfigured's-postinst> `abort-deconfigure' `in-favour'
#          <failed-install-package> <version> `removing'
#          <conflicting-package> <version>
# for details, see http://www.debian.org/doc/debian-policy/ or
# the debian-policy package


case "$1" in
    configure)

      nginx-disable-mod ach
      nginx-disable-mod calendar
      nginx-disable-mod cbb
      nginx-disable-mod center
      nginx-disable-mod default
      nginx-disable-mod exam
      nginx-disable-mod fotki
      nginx-disable-mod hideref
      nginx-disable-mod lego
      nginx-disable-mod maptiles
      nginx-disable-mod newhire
      nginx-disable-mod parking
      nginx-disable-mod search
      nginx-disable-mod staff
      nginx-disable-mod wiki
      nginx-disable-mod tm
      nginx-disable-mod oauth
      nginx-disable-mod atm
      nginx-disable-mod gant
      nginx-disable-mod smstools
      nginx-disable-mod tanker
      nginx-disable-mod askbot
      nginx-disable-mod help-otrs
      
      nginx-disable-mod agave
      nginx-enable-mod agave2
      nginx-enable-mod ask
      nginx-enable-mod at-yandex-team
      nginx-disable-mod beta
      nginx-enable-mod coalmine
      nginx-disable-mod doc
      nginx-disable-mod events
      nginx-enable-mod gap
      nginx-enable-mod gr
      nginx-enable-mod grng
      nginx-disable-mod notify
      nginx-enable-mod people
      nginx-disable-mod resize
      nginx-enable-mod sbcast
      nginx-enable-mod fontapi
      nginx-enable-mod team.yandex.st

    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
