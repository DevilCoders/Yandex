#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/yandex-juggler-reject-iptables-check/usr/bin/
	mkdir -p debian/yandex-juggler-reject-iptables-check/etc/monrun/conf.d/
	cp reject-iptables-check.sh debian/yandex-juggler-reject-iptables-check/usr/bin/
	cp reject-iptables.conf debian/yandex-juggler-reject-iptables-check/etc/monrun/conf.d/

%:
	dh $@

