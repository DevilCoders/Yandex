#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-corba-lpythonbp-ganglia/etc/logster/
	mkdir -p debian/config-corba-lpythonbp-ganglia/etc/cron.d/
	cp -r logster/* debian/config-corba-lpythonbp-ganglia/etc/logster/
	cp -r cron.d/* debian/config-corba-lpythonbp-ganglia/etc/cron.d/
%:
	dh $@

