#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-monitoring-logrotate-download/etc/monrun/conf.d/
	mkdir -p debian/config-monitoring-logrotate-download/usr/lib/config-monitoring-corba/
	cp logrotate.conf debian/config-monitoring-logrotate-download/etc/monrun/conf.d/
	cp logrotate.sh debian/config-monitoring-logrotate-download/usr/lib/config-monitoring-corba/
	chmod +x debian/config-monitoring-logrotate-download/usr/lib/config-monitoring-corba/logrotate.sh
%:
	dh $@

