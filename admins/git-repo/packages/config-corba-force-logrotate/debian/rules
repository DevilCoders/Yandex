#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-corba-force-logrotate/etc/cron.d/
	mkdir -p debian/config-corba-force-logrotate/usr/bin/
	cp force-logrotate.sh debian/config-corba-force-logrotate/usr/bin/
	cp force-logrotate.cron debian/config-corba-force-logrotate/etc/cron.d/force-logrotate

%:
	dh $@

