#!/usr/bin/make -f

build: 
	# Nothing to do

install:
	dh_testdir
	dh_testroot
	dh_clean -k
	$(CURDIR)/gen-make.pl $(CURDIR)
	$(MAKE) install DESTDIR=$(CURDIR)/debian/yandex-ocorba
	dh_link

binary-indep: build install
	dh_testdir
	dh_testroot
	dh_installdirs
	dh_installcron
	dh_installchangelogs
	dh_compress
	dh_fixperms
	dh_installdeb
	dh_gencontrol
	dh_builddeb

binary-arch: build install
	#Nothing to do

binary:  binary-indep binary-arch

clean: 
	dh_clean
