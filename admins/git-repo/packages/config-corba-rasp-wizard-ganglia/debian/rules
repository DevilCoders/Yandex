#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-corba-rasp-wizard-ganglia/etc/logster/
	mkdir -p debian/config-corba-rasp-wizard-ganglia/etc/cron.d/
	cp -r logster/* debian/config-corba-rasp-wizard-ganglia/etc/logster/
	cp -r cron.d/* debian/config-corba-rasp-wizard-ganglia/etc/cron.d/

%:
	dh $@
