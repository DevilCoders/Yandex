#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/yandex-juggler-dom0-checks/usr/bin/
	cp bin/* debian/yandex-juggler-dom0-checks/usr/bin/
	chmod +x debian/yandex-juggler-dom0-checks/usr/bin/*

	mkdir -p debian/yandex-juggler-dom0-checks/etc/monrun/conf.d/
	cp monrun-conf.d/* debian/yandex-juggler-dom0-checks/etc/monrun/conf.d/

%:
	dh $@

