#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/yandex-juggler-http-check/usr/bin/
	cp -r jhttp.sh debian/yandex-juggler-http-check/usr/bin/jhttp.sh
	cp -r http_check.sh debian/yandex-juggler-http-check/usr/bin/http_check.sh
	chmod +x debian/yandex-juggler-http-check/usr/bin/http_check.sh
	chmod +x debian/yandex-juggler-http-check/usr/bin/jhttp.sh
	mkdir -p debian/yandex-juggler-http-check/etc/monrun/conf.d/
	cp -r http_check.conf.example debian/yandex-juggler-http-check/etc/monrun/conf.d/

%:
	dh $@


