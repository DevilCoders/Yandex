#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-monitoring-http-corba-lpythonbp/etc/monrun/conf.d/
	cp jhttp.conf debian/config-monitoring-http-corba-lpythonbp/etc/monrun/conf.d/
	cp jmemcached.conf debian/config-monitoring-http-corba-lpythonbp/etc/monrun/conf.d/
	mkdir -p debian/config-monitoring-http-corba-lpythonbp/usr/sbin/
	cp http_api-yaru.yandex.ru.sh debian/config-monitoring-http-corba-lpythonbp/usr/sbin/http_api-yaru.yandex.ru.sh
%:
	dh $@

