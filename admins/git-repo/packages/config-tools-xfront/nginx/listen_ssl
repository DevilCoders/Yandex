listen [::]:443 ssl;
ssl_certificate /etc/nginx/ssl/wc.yandex-team.ru.crt;
ssl_certificate_key /etc/nginx/ssl/wc.yandex-team.ru.key;
ssl_prefer_server_ciphers on;
ssl_protocols SSLv3 TLSv1 TLSv1.1 TLSv1.2;
ssl_ciphers AES128-SHA:RC4-SHA:AES256-SHA:DES-CBC3-SHA:ECDH-RSA-RC4-SHA:ECDH-ECDSA-RC4-SHA:!MD5:HIGH:!aNULL:!DH:!PSK:!SSLv2;

