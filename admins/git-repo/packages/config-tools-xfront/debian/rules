#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-tools-xfront/etc/nginx/sites-enabled
	mkdir -p debian/config-tools-xfront/etc/monrun/conf.d/
	mkdir -p debian/config-tools-xfront/etc/cron.d/
	mkdir -p debian/config-tools-xfront/etc/logster/
	cp monrun-conf.d/* debian/config-tools-xfront/etc/monrun/conf.d/
	cp -r nginx debian/config-tools-xfront/etc/
	cp cron.d/* debian/config-tools-xfront/etc/cron.d/
	cp logster/* debian/config-tools-xfront/etc/logster/
	cp omniORB4.cfg  debian/config-tools-xfront/etc/
%:
	dh $@

