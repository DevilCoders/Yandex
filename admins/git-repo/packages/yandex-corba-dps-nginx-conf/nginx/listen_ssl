listen 443 ssl;
listen [::]:443 ssl;
listen 8080 ssl;
listen [::]:8080 ssl;
ssl_certificate      /etc/nginx/ssl/wc.yandex-team.ru.crt;
ssl_certificate_key  /etc/nginx/ssl/wc.yandex-team.ru.key;
ssl_prefer_server_ciphers on;
ssl_protocols TLSv1 SSLv3 TLSv1.1 TLSv1.2;
ssl_ciphers kRSA+AES128:kRSA:kEECDH:+3DES:!RC4:!aNULL:!eNULL:!MD5:!EXPORT:!LOW:!SEED:!CAMELLIA:!IDEA:!PSK:!SRP:!SSLv2;
ssl_session_cache    shared:SSL:64m;
ssl_session_timeout  12h;
