DESTDIR=/usr/local

install:
	install -d -m 755 $(DESTDIR)/etc/yandex/statbox-push-client/
	install -m 644 config-push-client-search.yaml $(DESTDIR)/etc/yandex/statbox-push-client/push-client-search.yaml
	install -d -m 755 $(DESTDIR)/etc/yandex/statbox-push-client-extra/
	install -m 644 push-client-search $(DESTDIR)/etc/yandex/statbox-push-client-extra/push-client-search

