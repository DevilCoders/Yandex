install:
	install -m 755 -d $(DESTDIR)//usr/local/sbin/
	install -m 755 reblogs $(DESTDIR)//usr/local/sbin/reblogs
	install -m 755 -d $(DESTDIR)//etc/
	install -m 644 rsyncd.conf $(DESTDIR)//etc/rsyncd.conf
	install -m 755 -d $(DESTDIR)//etc/hcheck/
	install -m 644 services.conf $(DESTDIR)//etc/hcheck/services.conf
