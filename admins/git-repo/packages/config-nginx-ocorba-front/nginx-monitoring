[nginx-is-running]
execution_interval=300
command=/usr/local/sbin/nginx-is-running.sh

[500-http-nginx]
execution_interval=120
command=/usr/bin/http.pl -f /var/log/nginx/access.log

[balancer-is-here]
execution_interval=60
command=/usr/bin/jbalancerishere.sh

[balancer_reject]
execution_interval=300
command=/usr/bin/monrun_balancer_reject.sh monrun

