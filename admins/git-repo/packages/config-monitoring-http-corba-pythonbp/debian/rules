#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-monitoring-http-corba-pythonbp/etc/monrun/conf.d/
	cp jhttp.conf debian/config-monitoring-http-corba-pythonbp/etc/monrun/conf.d/
	cp jcert.conf debian/config-monitoring-http-corba-pythonbp/etc/monrun/conf.d/	
	cp jrsync.conf debian/config-monitoring-http-corba-pythonbp/etc/monrun/conf.d/
	cp jmemcached.conf debian/config-monitoring-http-corba-pythonbp/etc/monrun/conf.d/

%:
	dh $@

