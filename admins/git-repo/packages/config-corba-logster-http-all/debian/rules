#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-corba-logster-http-all/etc/logster/
	mkdir -p debian/config-corba-logster-http-all/etc/cron.d/
	cp logster-default-http.conf debian/config-corba-logster-http-all/etc/logster/
	cp logster-default-http.cron debian/config-corba-logster-http-all/etc/logster/

%:
	dh $@

