#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/yandex-juggler-conntrackissmall-check/usr/bin/
	mkdir -p debian/yandex-juggler-conntrackissmall-check/etc/monrun/conf.d/
	cp jconntrackissmall.sh debian/yandex-juggler-conntrackissmall-check/usr/bin/
	cp jconntrackissmall.conf debian/yandex-juggler-conntrackissmall-check/etc/monrun/conf.d/

%:
	dh $@

