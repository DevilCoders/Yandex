#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/config-download.yandex.ru-sync/usr/bin/
	mkdir -p debian/config-download.yandex.ru-sync/etc/cron.d/
	mkdir -p debian/config-download.yandex.ru-sync/etc/monrun/conf.d/
	cp dndl-sync-ng.sh debian/config-download.yandex.ru-sync/usr/bin/
	cp dndl-sync-ng-cron debian/config-download.yandex.ru-sync/etc/cron.d/
	cp dndl-sync-check.sh debian/config-download.yandex.ru-sync/usr/bin/
	cp dndl-sync-check.conf debian/config-download.yandex.ru-sync/etc/monrun/conf.d/

%:
	dh $@

