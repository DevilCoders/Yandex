#!/usr/bin/make -f
override_dh_install:
	mkdir -p debian/yandex-juggler-zookeeper-check/usr/bin/
	mkdir -p debian/yandex-juggler-zookeeper-check/etc/monrun/conf.d/
	cp jzookeeperalive.sh debian/yandex-juggler-zookeeper-check/usr/bin/
	cp jzookeeperalive.conf debian/yandex-juggler-zookeeper-check/etc/monrun/conf.d/

%:
	dh $@

